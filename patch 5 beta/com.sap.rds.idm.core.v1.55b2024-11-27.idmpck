<MCSCRIPTDEFINITION> function sapc_core_shuffleString(Par) {
     //							- Input String
     //							- Output shuffled string
     var array = Par.split("");
     var tmp, current, top = array.length;

     if (top)
         while (--top) {
             current = Math.floor(Math.random() * (top + 1));
             tmp = array[current];
             array[current] = array[top];
             array[top] = tmp;
         }

     return array.join("");
 }</MCSCRIPTDEFINITION>
