<MCSCRIPTDEFINITION>// Main function:sapc_getFolderNameFromActivityText

function sapc_getFolderNameFromActivityText(activityText) {
    uInfo("sapc_getFolderNameFromActivityText: Started for Job [" + activityText + "]");
    if (activityText.toUpperCase().indexOf("IDENTITY") != -1) return "IDENTITY";
    else if (activityText.toUpperCase().indexOf("CONFIGITEM") != -1) return "CONFIGITEM";
    else if (activityText.toUpperCase().indexOf("ROLE") != -1) return "ROLE";
    else if (activityText.toUpperCase().indexOf("NOTIFICATION") != -1) return "NOTIFICATION";
    else if (activityText.toUpperCase().indexOf("RULE_ENGINE") != -1) return "RULE_ENGINE";
    else if (activityText.toUpperCase().indexOf("TRANSLATION") != -1) return "TRANSLATION";
    else if (activityText.toUpperCase().indexOf("VALUEHELP") != -1) return "VALUEHELP";
    else {
        uWarning("sapc_getFolderNameFromActivityText: Could not determine download folder name for job [" + activityText + "]");
        return "";
    }
}</MCSCRIPTDEFINITION>
