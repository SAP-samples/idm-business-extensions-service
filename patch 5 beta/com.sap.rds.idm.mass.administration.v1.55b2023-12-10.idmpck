<mx:Function Enabled="TRUE" ErrorStatus="0" Script="JScript" name="sapc_getGroupedEntries">
<mx:Code>/*
 * 2022-05-13 SAP NN:	Get all roles / privs to be added / deleted at one time for better grouping
 * IN:					USERID, AUTHTYPE, OPERATION, ADDPARAMETERS (e.g. ADMINISTRATOR, ROLE, DELETE, VALIDFROM=2022-05-16!!REASON=abc)
 * OUT:					{x}&lt;123&gt;|&lt;345&gt;
 * 2022-05-24 SAP NN:	added LinkAssigner if not provided as parameter
 */

function sapc_getGroupedEntries(Par) {
    var parArray = Par.split("!!");
    var userid = parArray[0];
    var authType = parArray[1];
    var operationText = parArray[2];
    var addParameterPar = "" + parArray[3];
    if (!addParameterPar || addParameterPar == "undefined") addParameterPar = ""; //2022-06-01 SAP NN: sometimes its just undefined as text, for whatever reason
    var authTypeText = "PRIVILEGE";
    if (authType == "ROLE") authTypeText = "BUSINESS_ROLE";
    var operation = "{A}";
    if (operationText == "DELETE") operation = "{e}";
    var nolock = sapc_core_getNolock();
    var retString = "";
    var requestor = sapc_getJobVar("SAPC_REQ_REQUESTOR");

    var sql = "select " + operationText + "_" + authTypeText + ", valid_from, valid_to, additional_parameters from SAPC_IDENTITY_UPLOAD_ASSIGN " + nolock + " ";
    sql += " WHERE ((" + operationText + "_" + authTypeText + " IS NOT NULL) AND (" + sapc_core_getDBcommands("STRINGLENGTH") + "(" + operationText + "_" + authTypeText + ") &gt; 1)) ";
    sql += "  AND USERID = '" + userid + "' ";
    uInfo("sapc_getGroupedEntries: Query to get [" + authType + "] for User [" + userid + "] and operation [" + operation + "]: [" + sql + "]");

    var auths = uSelect(sql);

    // loop through all values and check existence of role and print error if it does not exist 
    var authsArray = auths.split("!!");


    for (iai = 0; iai &lt; authsArray.length; iai++) {
        var cEntry = authsArray[iai].split("|");
        sql = "SELECT mcmskey FROM idmv_entry_simple " + nolock + " WHERE mcMSKEYVALUE = UPPER('" + cEntry[0] + "') AND mcIdStore = " + uGetIDStore();
        var roleMSKEY = uSelect(sql);
        var additionalParameters = "" + addParameterPar;
        if (cEntry[3] &amp;&amp; cEntry[3] != "null") {
            if (addParameterPar != "") addParameterPar += "!!";
            additionalParameters += cEntry[3];
        }

        if (!roleMSKEY || roleMSKEY.indexOf("!ERROR") != -1) {
            // No entry found; writing error message to the log
            uErrMsg(1, "sapc_getGroupedEntries: Current MskeyValue [" + authsArray[iai] + "] has not been found.");
            uSkip(1, 2, "ENTRY [" + authsArray[iai] + "] NOT FOUND");
            return 0;

        } else {

            //VALIDFROM=$FUNCTION.sapc_convertValidDate($FUNCTION.sapc_setDefaultValidDate(%VALID_FROM%|VALID_FROM)$$)$$!!VALIDTO=$FUNCTION.sapc_convertValidDate($FUNCTION.sapc_setDefaultValidDate(%VALID_TO%|VALID_TO)$$)$$
            if (operationText == "ADD") {
                if (additionalParameters) additionalParameters += "!!";
                additionalParameters += "VALIDFROM=" + sapc_convertValidDate(sapc_setDefaultValidDate(cEntry[1] + "|VALID_FROM")) + "!!VALIDTO=" + sapc_convertValidDate(sapc_setDefaultValidDate(cEntry[2] + "|VALID_TO"));
            }

            // add link assigner if not provided as par
            if (operationText == "ADD" &amp;&amp; additionalParameters &amp;&amp; additionalParameters.toLowerCase().indexOf("linkassigner") == -1) {
                if (additionalParameters) additionalParameters += "!!";
                additionalParameters += "LinkAssigner=" + requestor;
            }

            uInfo("sapc_getGroupedEntries: Additional Assignment Parameters for [" + authsArray[iai] + "]: [" + additionalParameters + "]");

            if (retString != "") retString += "|";
            if (additionalParameters) retString += "{" + additionalParameters + "}";
            retString += roleMSKEY;
        }
    }

    if (retString == "") return "{A}";
    else return operation + retString;
}</mx:Code>
</mx:Function>
</mx:Functions>
