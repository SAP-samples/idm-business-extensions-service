<mx:Function Enabled="TRUE" ExternalScript="com.sap.rds.idm.core:jscript.sapc_core_script_functions" Script="JScript" name="sapc_core_script_functions"/>
<mx:Function Enabled="TRUE" ErrorStatus="0" Script="JScript" name="sapc_copyFileToShare">
<mx:Code>/*
 * 2020-10-27 SAP NN:	Adaptations for SP4 - set file name on request, not on config item
 * 2022-06-21 SAP NN:	outsourced activityText to folder name into separate script sapc_getFolderNameFromActivityText
 *						if (fileAttribute || fileAttribute != "null") { ... -&gt; if (fileAttribute &amp;&amp; fileAttribute != "null") {
 * Dependencies:		sapc_getFolderNameFromActivityText
 */

function sapc_copyFileToShare(Par) {
    var rds = sapc_core_scriptHeader("sapc_copyFileToShare", Par);

    var mskey = Par.get("MSKEY");
    var requestType = Par.get("ACTIVITY");
    var fileAttribute = Par.get("FILE_ATTRIBUTE");
    var activityText = Par.get("ACTIVITY_TEXT");
    var fileName = "";
    var binary = "";
    var folderName = "";

    /* changed with SP4, folder name not identified by job name anymore this way
       var arrReqType = activityText.split("_JOB_");
       var reqPart = arrReqType[1];
       var index_ = reqPart.indexOf("_");
       var folderName = reqPart.substring(0, index_);
       */
    folderName = sapc_getFolderNameFromActivityText(activityText);

    var basePath = (activityText.toUpperCase().indexOf("DOWNLOAD") &gt; 0) ? uGetConstant("pck.SAPC_PATH_DOWNLOAD") : uGetConstant("pck.SAPC_PATH_UPLOAD");

    var filePath = basePath;

    if (folderName.length != 0)
        filePath += "\\" + folderName;


    var sql = "";
    var changeType = "";
    var dbType = "%$ddm.databasetype%";

    uErrMsg(1, rds.outPrefix + "FileAttribute: " + fileAttribute);

    if (!fileAttribute || fileAttribute == "null") {
        uWarning(rds.outPrefix + "No upload file provided. Assuming job is a download job.");
        // new String required to prevent java.lang.Throwable: Cannot convert /[!@#$%\^&amp;*(),.?\":{}|&lt;&gt;\/\\]/g to java.lang.Character
        fileName = ("" + activityText).replace(/[!@#$%\^&amp;*(),.?\":{}|&lt;&gt;\/\\]/g, "_") + ".csv";
    } else {
        // get filename from binary
        var fileArray = fileAttribute.split(":");
        fileName = fileArray[0];
        binary = fileArray[1];
    }

    if (rds.logLevel &gt;= 1) uErrMsg(1, rds.outPrefix + " ; File name: " + fileName + " ; Binary: " + binary);


    var canonicalPath = filePath + "\\" + uGetAuditID() + "_" + fileName;

    if (fileAttribute &amp;&amp; fileAttribute != "null") {
        var filePlain = uFromHex(binary);
        if (rds.logLevel &gt;= 2) uErrMsg(0, rds.outPrefix + "Plain: " + filePlain);
        uToFile(canonicalPath, filePlain, "FALSE");
    }

    // 2020-10-27 SAP NN: Adaptations for SP4 - set file name on request, not on config item
    //sapc_setConfigItem("SAPC_JOB_FILEPATH!!" + requestType + "!!" + canonicalPath);
    uIS_SetValue(mskey, rds.masterIDS, "SAPC_REQ_TRIGGER_MASS_CHANGE_FILE_PATH", canonicalPath, rds.taskName);

    uErrMsg(1, rds.outPrefix + " File has been downloaded successfully to defined Share [" + canonicalPath + "]");

    return "";
}</mx:Code>
</mx:Function>
