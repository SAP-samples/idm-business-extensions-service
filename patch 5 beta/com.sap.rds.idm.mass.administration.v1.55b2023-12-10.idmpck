<mx:Function Enabled="TRUE" ErrorStatus="0" Script="JScript" name="sapc_triggerDisableEnableFromJob">
<mx:Code>// Main function: sapc_triggerDisableEnableFromJob
// Global Operation: DISABLE or ENABLE
// 2020-10-29 SAP NN: Adaptations for SP4 (no config items)
// 2022-08-29 SAP NN: exclude ACCOUNTMETA% when checking for users accounts

function sapc_triggerDisableEnableFromJob(Par) {

    // Input data
    var userMSKEYVALUE = Par.get("MSKEYVALUE");
    var currentDisableRepository = Par.get("REP_DISABLE");
    var currentEnableRepository = Par.get("REP_ENABLE");
    var globalOperation = Par.get("GLOBAL_OPERATION");
    var operationTimeAndReason = Par.get("TIME_AND_REASON") + "";

    var logLevel = sapc_core_getPackageConstant("SAPC_LOG_LEVEL");
    var currentIDStore = sapc_core_getPackageConstant("SAPC_MASTER_IDS_ID");
    var script = "sapc_triggerDisableEnableFromJob ";

    // New variables set in the context below
    var disableRepositories = "";
    var enableRepositories = "";
    var curRepPackageID = "";
    var disableAudit = "";
    var accountRepositories = "";

    // Set disableReason, disableTime
    var operationTimeAndReasonSplit = operationTimeAndReason.split("!!");
    var disableTime = operationTimeAndReasonSplit[0];
    var disableReason = operationTimeAndReasonSplit[1];
    var disableVarName = "";
    var result = "";

    // Get mskey of user
    var userMSKEY = uIS_Get(currentIDStore, userMSKEYVALUE);
    var MX_DISABLED = uIS_GetValue(userMSKEY, currentIDStore, "MX_DISABLED");

    var disablePerformedByMSKEY = sapc_getJobVar("SAPC_REQ_REQUESTOR");
    uInfo("sapc_triggerDisableEnableFromJob: disablePerformedByMSKEY: " + disablePerformedByMSKEY);
    var disablePerformedByMSKEYVALUE = uIS_GetValue(disablePerformedByMSKEY, currentIDStore, "MSKEYVALUE");
    var provInfo = "Performed via Upload Job executed by " + disablePerformedByMSKEYVALUE;

    // Log output for error analysis
    if (logLevel &gt;= 3) {
        UserFunc.uErrMsg(1, script + " userMSKEY: " + userMSKEY);
        UserFunc.uErrMsg(1, script + " user MX_DISABLED value: " + MX_DISABLED);
        UserFunc.uErrMsg(1, script + " called for userMSKEYVALUE: " + userMSKEYVALUE);
        UserFunc.uErrMsg(1, script + " called for userMSKEY: " + userMSKEY);
        UserFunc.uErrMsg(1, script + " globalOperation: " + globalOperation);
        UserFunc.uErrMsg(1, script + " currentDisableRepository: " + currentDisableRepository);
        UserFunc.uErrMsg(1, script + " currentEnableRepository: " + currentEnableRepository);
        UserFunc.uErrMsg(1, script + " operationTimeAndReason: " + operationTimeAndReason);
    }

    // Create SQL statement to retrieve all repositories of the user
    if (globalOperation == "DISABLE" || globalOperation == "ENABLE") {
        // SQL server ONLY
        var SQL = "SELECT SUBSTRING(attrname,8,20) AS repositories FROM idmv_value_basic_active ";
        SQL += "WHERE attrname LIKE 'ACCOUNT%' AND attrname NOT LIKE 'ACCOUNTMETA%' AND mskey = " + userMSKEY + " ORDER BY attrname";
        if (logLevel &gt;= 3) {
            UserFunc.uErrMsg(1, script + " SQL to get all repositories: " + SQL);
        }
        accountRepositories = UserFunc.uSelect(SQL);
        // Replace !! by | as seperator
        accountRepositories = UserFunc.uReplaceString(accountRepositories, "!!", "|");
        if (logLevel &gt;= 3) {
            UserFunc.uErrMsg(1, script + " accountRepositories: " + accountRepositories);
        }
    }

    // If identity needs to be disabled on all repositories -&gt; get the repositories and start the provisioning
    if (globalOperation == "DISABLE") {
        disableRepositories = disableRepositories + accountRepositories;
        if (logLevel &gt;= 3) {
            UserFunc.uErrMsg(1, script + " disableRepositories: " + disableRepositories);
        }
        // Set MX_DISABLED
        OutString = uIS_SetValue(userMSKEY, currentIDStore, "MX_DISABLED", "1", disablePerformedByMSKEY);
        if (logLevel &gt;= 3) {
            UserFunc.uErrMsg(1, script + " setting MX_DISABLED for user: " + userMSKEYVALUE);
        }

        // Processing disableRepositories
        if (disableRepositories != "" &amp;&amp; disableRepositories != null) {
            // Remove last | from disableRepositories
            var disableRepositoriesLength = disableRepositories.length;
            var disableRepositoriesLastChar = disableRepositories.charAt(disableRepositoriesLength - 1) + "";
            if (disableRepositoriesLastChar == "|") {
                disableRepositories = disableRepositories.slice(0, disableRepositoriesLength - 1);
                if (logLevel &gt;= 3) {
                    UserFunc.uErrMsg(1, script + " removing last | from disableRepositories");
                }
            }
            if (logLevel &gt;= 3) {
                UserFunc.uErrMsg(1, script + " processing disableRepositories");
            }
            disableRepositoriesSplit = disableRepositories.split("|");
            for (i = 0; i &lt; disableRepositoriesSplit.length; i++) {
                currentDisableRepository = "";
                currentDisableRepository = disableRepositoriesSplit[i];
                curRepPackageID = "";
                curRepPackageID = sapc_core_getRepositoryPackageID(currentDisableRepository);
                if (logLevel &gt;= 3) {
                    UserFunc.uErrMsg(1, script + " currentDisableRepository: " + currentDisableRepository);
                }
                if (currentDisableRepository.length &gt; 0) {
                    // Set system specific disable attribute SAPC_IDEN_REP_DISABLED_&lt;repositoryname&gt;
                    disableVarName = "";
                    disableVarName = "SAPC_IDEN_REP_DISABLED_" + currentDisableRepository;
                    retValue = uIS_SetValue(userMSKEY, currentIDStore, disableVarName, 1, disablePerformedByMSKEY, 0);
                    if (logLevel &gt;= 3) {
                        UserFunc.uErrMsg(1, script + " updating " + disableVarName);
                    }
                    // Execute disable task of repository --&gt; DisableUser process of repository
                    disableIdentityTask = "" + uGetPackageTaskID(curRepPackageID, "DisableUser");

                    if (logLevel &gt;= 3) {
                        UserFunc.uErrMsg(1, script + " disableIdentityTask: " + disableIdentityTask);
                    }
                    result = UserFunc.uProvision(userMSKEY, disableIdentityTask, 0, (sapc_getRepID(currentDisableRepository)), provInfo, 0);
                    // Adding current repository to attribute SAPC_IDEN_REPOSITORIES_DISABLED and SAPC_IDEN_REPOSITORIES_DISABLED_UI
                    OutString = uIS_SetValue(userMSKEY, currentIDStore, "SAPC_IDEN_REPOSITORIES_DISABLED", currentDisableRepository, disablePerformedByMSKEY);
                    if (logLevel &gt;= 3) {
                        UserFunc.uErrMsg(1, script + " adding to SAPC_IDEN_REPOSITORIES_DISABLED");
                    }
                    OutString = uIS_SetValue(userMSKEY, currentIDStore, "SAPC_IDEN_REPOSITORIES_DISABLED_UI", currentDisableRepository, disablePerformedByMSKEY);
                    if (logLevel &gt;= 3) {
                        UserFunc.uErrMsg(1, script + " adding to SAPC_IDEN_REPOSITORIES_DISABLED_UI");
                    }
                }
            }
        }
    }

    // If identity needs to be enabled on all repositories -&gt; get the repositories and start the provisioning
    else if (globalOperation == "ENABLE") {
        enableRepositories = enableRepositories + accountRepositories;
        if (logLevel &gt;= 3) {
            UserFunc.uErrMsg(1, script + " enableRepositories: " + enableRepositories);
        }
        // remove MX_DISABLED if it has been set
        if (MX_DISABLED == "1") {
            OutString = uIS_SetValue(userMSKEY, currentIDStore, "MX_DISABLED", "1", disablePerformedByMSKEY, 2);
            if (logLevel &gt;= 3) {
                UserFunc.uErrMsg(1, script + " removing MX_DISABLED for user: " + userMSKEYVALUE);
            }
        }

        // Processing enableRepositories
        if (enableRepositories != "" &amp;&amp; enableRepositories != null) {
            // Remove last | from enableRepositories
            var enableRepositoriesLength = enableRepositories.length;
            var enableRepositoriesLastChar = enableRepositories.charAt(enableRepositoriesLength - 1) + "";
            if (enableRepositoriesLastChar == "|") {
                enableRepositories = enableRepositories.slice(0, enableRepositoriesLength - 1);
                if (logLevel &gt;= 3) {
                    UserFunc.uErrMsg(1, script + " removing last | from enableRepositories");
                }
            }
            if (logLevel &gt;= 3) {
                UserFunc.uErrMsg(1, script + " processing enableRepositories");
            }
            enableRepositoriesSplit = enableRepositories.split("|");
            for (i = 0; i &lt; enableRepositoriesSplit.length; i++) {
                currentEnableRepository = "";
                currentEnableRepository = enableRepositoriesSplit[i];
                curRepPackageID = "";
                curRepPackageID = sapc_core_getRepositoryPackageID(currentEnableRepository);
                if (logLevel &gt;= 3) {
                    UserFunc.uErrMsg(1, script + " currentEnableRepository: " + currentEnableRepository);
                }
                if (currentEnableRepository.length &gt; 0) {
                    // Remove system specific disable attribute SAPC_IDEN_REP_DISABLED_&lt;repositoryname&gt;
                    disableVarName = "";
                    disableVarName = "SAPC_IDEN_REP_DISABLED_" + currentEnableRepository;
                    retValue = uIS_SetValue(userMSKEY, currentIDStore, disableVarName, "", disablePerformedByMSKEY, 0);
                    if (logLevel &gt;= 3) {
                        UserFunc.uErrMsg(1, script + " updating " + disableVarName);
                    }
                    // Execute enable task of repository --&gt; EnableUser process of repository
                    enableIdentityTask = "" + uGetPackageTaskID(curRepPackageID, "EnableUser");

                    if (logLevel &gt;= 3) {
                        UserFunc.uErrMsg(1, script + " enableIdentityTask: " + enableIdentityTask);
                    }
                    result = UserFunc.uProvision(userMSKEY, enableIdentityTask, 0, (sapc_getRepID(currentEnableRepository)), provInfo, 0);
                    // Removing current repository from attribute SAPC_IDEN_REPOSITORIES_DISABLED and SAPC_IDEN_REPOSITORIES_DISABLED_UI
                    OutString = uIS_SetValue(userMSKEY, currentIDStore, "SAPC_IDEN_REPOSITORIES_DISABLED", currentEnableRepository, disablePerformedByMSKEY, 2);
                    if (logLevel &gt;= 3) {
                        UserFunc.uErrMsg(1, script + " removing from SAPC_IDEN_REPOSITORIES_DISABLED");
                    }
                    OutString = uIS_SetValue(userMSKEY, currentIDStore, "SAPC_IDEN_REPOSITORIES_DISABLED_UI", currentEnableRepository, disablePerformedByMSKEY, 2);
                    if (logLevel &gt;= 3) {
                        UserFunc.uErrMsg(1, script + " removing from SAPC_IDEN_REPOSITORIES_DISABLED_UI");
                    }
                }
            }
        }
    }

    // If a concrete repository has been defined
    else {

        // Process disable repository
        if (currentDisableRepository != null &amp;&amp; currentDisableRepository != "") {
            curRepPackageID = "";
            curRepPackageID = sapc_core_getRepositoryPackageID(currentDisableRepository);
            // Set system specific disable attribute SAPC_IDEN_REP_DISABLED_&lt;repositoryname&gt;
            disableVarName = "";
            disableVarName = "SAPC_IDEN_REP_DISABLED_" + currentDisableRepository;
            retValue = uIS_SetValue(userMSKEY, currentIDStore, disableVarName, 1, disablePerformedByMSKEY, 0);
            if (logLevel &gt;= 3) {
                UserFunc.uErrMsg(1, script + " updating " + disableVarName);
            }
            disableIdentityTask = "" + uGetPackageTaskID(curRepPackageID, "DisableUser");

            if (logLevel &gt;= 3) {
                UserFunc.uErrMsg(1, script + " disableIdentityTask: " + disableIdentityTask);
            }
            result = UserFunc.uProvision(userMSKEY, disableIdentityTask, 0, (sapc_getRepID(currentDisableRepository)), provInfo, 0);
            // Adding current repository to attribute SAPC_IDEN_REPOSITORIES_DISABLED and SAPC_IDEN_REPOSITORIES_DISABLED_UI
            OutString = uIS_SetValue(userMSKEY, currentIDStore, "SAPC_IDEN_REPOSITORIES_DISABLED", currentDisableRepository, disablePerformedByMSKEY);
            if (logLevel &gt;= 3) {
                UserFunc.uErrMsg(1, script + " adding to SAPC_IDEN_REPOSITORIES_DISABLED");
            }
            OutString = uIS_SetValue(userMSKEY, currentIDStore, "SAPC_IDEN_REPOSITORIES_DISABLED_UI", currentDisableRepository, disablePerformedByMSKEY);
            if (logLevel &gt;= 3) {
                UserFunc.uErrMsg(1, script + " adding to SAPC_IDEN_REPOSITORIES_DISABLED_UI");
            }
            // Prepare data for audit
            disableRepositories = currentDisableRepository + "";
        }

        // Process enable repository
        if (currentEnableRepository != null &amp;&amp; currentEnableRepository != "") {
            curRepPackageID = "";
            curRepPackageID = sapc_core_getRepositoryPackageID(currentEnableRepository);
            // Delete MX_DISABLED because user cannot remain globally locked anymore
            if (MX_DISABLED == "1" || MX_DISABLED == 1 || MX_DISABLED == "TRUE") {
                // Delete MX_DISABLED
                OutString = uIS_SetValue(userMSKEY, currentIDStore, "MX_DISABLED", "", disablePerformedByMSKEY);
                if (logLevel &gt;= 3) {
                    UserFunc.uErrMsg(1, script + " deleting MX_DISABLED for user: " + userMSKEYVALUE);
                }
            }
            // Remove system specific disable attribute SAPC_IDEN_REP_DISABLED_&lt;repositoryname&gt;
            disableVarName = "";
            disableVarName = "SAPC_IDEN_REP_DISABLED_" + currentEnableRepository;
            retValue = uIS_SetValue(userMSKEY, currentIDStore, disableVarName, "", disablePerformedByMSKEY, 0);
            if (logLevel &gt;= 3) {
                UserFunc.uErrMsg(1, script + " updating " + disableVarName);
            }
            enableIdentityTask = "" + uGetPackageTaskID(curRepPackageID, "EnableUser");

            if (logLevel &gt;= 3) {
                UserFunc.uErrMsg(1, script + " enableIdentityTask: " + enableIdentityTask);
            }
            result = UserFunc.uProvision(userMSKEY, enableIdentityTask, 0, (sapc_getRepID(currentEnableRepository)), provInfo, 0);
            // Removing current repository from attribute SAPC_IDEN_REPOSITORIES_DISABLED and SAPC_IDEN_REPOSITORIES_DISABLED_UI
            OutString = uIS_SetValue(userMSKEY, currentIDStore, "SAPC_IDEN_REPOSITORIES_DISABLED", currentEnableRepository, disablePerformedByMSKEY, 2);
            if (logLevel &gt;= 3) {
                UserFunc.uErrMsg(1, script + " removing from SAPC_IDEN_REPOSITORIES_DISABLED");
            }
            OutString = uIS_SetValue(userMSKEY, currentIDStore, "SAPC_IDEN_REPOSITORIES_DISABLED_UI", currentEnableRepository, disablePerformedByMSKEY, 2);
            if (logLevel &gt;= 3) {
                UserFunc.uErrMsg(1, script + " removing from SAPC_IDEN_REPOSITORIES_DISABLED_UI");
            }
            // Prepare data for audit
            enableRepositories = currentEnableRepository + "";
        }
    }

    // Prepare data and set SAPC_IDEN_DISABLE_AUDIT
    //var disablePerformedByMSKEYVALUE = "IDM_UPLOAD";

    if (globalOperation == "DISABLE" || globalOperation == "ENABLE") {
        // do nothing
    } else {
        globalOperation = "n/a";
    }
    disableAudit = disableAudit + "Timestamp: " + disableTime + "\n";
    disableAudit = disableAudit + "Disable performed on system(s): " + disableRepositories + "\n";
    disableAudit = disableAudit + "Enable performed on system(s): " + enableRepositories + "\n";
    disableAudit = disableAudit + "Global Operation: " + globalOperation + "\n";
    disableAudit = disableAudit + "Operation performed by: " + disablePerformedByMSKEYVALUE + "\n";
    disableAudit = disableAudit + "Reason: " + disableReason;
    if (logLevel &gt;= 3) {
        UserFunc.uErrMsg(1, script + " disableAudit: " + disableAudit);
    }
    OutString = uIS_SetValue(userMSKEY, currentIDStore, "SAPC_IDEN_DISABLE_AUDIT", disableAudit, disablePerformedByMSKEY);

    return "";
}</mx:Code>
