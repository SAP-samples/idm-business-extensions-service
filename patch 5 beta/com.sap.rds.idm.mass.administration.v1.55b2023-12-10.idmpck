<MCSCRIPTDEFINITION>/*
 * 2020-10-29 SAP NN: writing values like executor and file path into job constants, not not constantly calculate the values at runtime in destination of passes
 * Required constants:
 *  SAPC_REQ_REQUESTOR, SAPC_REQ_ACTIVITY, SAPC_REQ_TRIGGER_MASS_CHANGE_FILE_PATH
 * Dependencies:
 *  sapc_getRequestMSKEY, sapc_core_script_functions, sapc_getFolderNameFromActivityText
 *  2022-06-17 SAP NN: script doesnt work for download jobs if there is no file atribute on the request object. -&gt; get folder name from Job name
 *  2023-11-21 SAP NN: 2nd parameter of uInfo removed (useless)
 */

function sapc_setInitialJobConstants(Par) {

    var idStore = uGetIDStore();
    var reqMSKEY = sapc_getRequestMSKEY();
    uInfo("sapc_setInitialJobConstants: reqMSKEY: [" + reqMSKEY + "]");

    if (!reqMSKEY || isNaN(reqMSKEY) || reqMSKEY &lt; 1) {
        uErrMsg(2, "sapc_setInitialJobConstants: unable to get MSKEY of request");
        uStop("Unable to get MSKEY of request");
    }

    var requestor = uIS_GetValue(reqMSKEY, idStore, "SAPC_REQ_REQUESTOR");
    uInfo("sapc_setInitialJobConstants: reqMSKEY: [" + reqMSKEY + "]");

    var activity = uIS_GetValue(reqMSKEY, idStore, "SAPC_REQ_TRIGGER_MASS_CHANGE_ACTIVITY");
    uInfo("sapc_setInitialJobConstants: activity: [" + activity + "]");

    var filePath = uIS_GetValue(reqMSKEY, idStore, "SAPC_REQ_TRIGGER_MASS_CHANGE_FILE_PATH");
    if (filePath == "!ERROR:No such attribute") {
        uInfo("sapc_setInitialJobConstants: filePath is empty considering this a download job");
        //C:\usr\sap\IDM\IDM_WORKAREA\DOWNLOAD\CONFIGTEM
        var jobName = uSelect("select name from mc_jobs where jobguid = '" + activity + "'");
        uInfo("sapc_getDownloadFolderName: Name of Job with jobGuid [" + activity + "]: [" + jobName + "]");
        filePath = sapc_core_getPackageContant("SAPC_PATH_DOWNLOAD", "com.sap.rds.idm.mass.administration") + "\\" + sapc_getFolderNameFromActivityText(jobName) + "\\";
    }
    uInfo("sapc_setInitialJobConstants: filePath: [" + filePath + "]");


    uSetUserVar("SAPC_REQ_REQUESTOR", requestor);
    uSetUserVar("SAPC_REQ_ACTIVITY", activity);
    uSetUserVar("SAPC_REQ_TRIGGER_MASS_CHANGE_FILE_PATH", filePath);


}</MCSCRIPTDEFINITION>
