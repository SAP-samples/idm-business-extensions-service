<mx:Function Enabled="TRUE" ErrorStatus="0" Script="JScript" name="sapc_triggerJobExecution">
<mx:Code>function sapc_triggerJobExecution(Par) {

    var rds = sapc_core_scriptHeader("sapc_triggerJobExecution", Par);

    var mskey = Par.get("MSKEY");
    var requestorMskey = Par.get("REQUESTOR_MSKEY");
    var activity = Par.get("ACTIVITY");
    var SAPC_REQ_JOB_ID = Par.get("SAPC_REQ_JOB_ID");
    //var SAPC_REQ_JOB_GUID = Par.get("SAPC_REQ_JOB_GUID");
    var DATETIME = Par.get("DATETIME");
    var requestorMskeyValue = uIS_sGetValue(requestorMskey, rds.masterIDS, "MSKEYVALUE", 0);

    var returnValue = uRunJobNow(SAPC_REQ_JOB_ID);

    if (returnValue.equals("")) {
        uErrMsg(0, rds.outPrefix + "Job [" + activity + "] with ID " + SAPC_REQ_JOB_ID + " has been triggered successfully");
        uSetContextVar("SAPC_REQ_STATE", "RUNNING");
        uAppContextVar("SAPC_REQ_STATE_INFO", "|[" + rds.taskName + "/" + DATETIME + "] Job [" + activity + "/(" + SAPC_REQ_JOB_ID + ")] , started by [" + requestorMskeyValue + "] set to state running.");
    } else {
        uErrMsg(2, rds.outPrefix + "Job [" + activity + "] with ID " + SAPC_REQ_JOB_ID + " has NOT been triggered successfully");
        uSetContextVar("SAPC_REQ_STATE", "ERROR");
        uAppContextVar("SAPC_REQ_STATE_INFO", "|[" + rds.taskName + "/" + DATETIME + "] Job [" + activity + "/(" + SAPC_REQ_JOB_ID + ")] , initialized by [" + requestorMskeyValue + "] could not be started. Error Message is: " + returnValue);
    }
}</mx:Code>
</mx:Function>
</mx:Functions>
