// 2020-10-29 SAP NN: SAPC_%$ddm.mcjob%_FILEPATH -&gt; SAPC_REQ_TRIGGER_MASS_CHANGE_FILE_PATH

function sapc_setFileAttributeContentForRequest(Par) {

    var rds = sapc_core_scriptHeader("sapc_setFileAttributeContentForRequest", Par);
    var canonicalFileName = sapc_getJobVar("SAPC_REQ_TRIGGER_MASS_CHANGE_FILE_PATH"); //sapc_getFileNameForMassOperation();

    // caninicalPathName contains _AuditID()_fileName;
    var lastIndexOf_ = canonicalFileName.lastIndexOf("_");

    if (lastIndexOf_ &lt; 1) {
        uError(rds.outPrefix + "fileName: [" + canonicalFileName + "] is not compatible");
        return "NULLATTR";
    }
    var fileName = canonicalFileName.substring(lastIndexOf_ + 1);

    var fileContent = uFromFile(canonicalFileName, -1, false);

    var fileBinary = uToHex(fileContent);

    uInfo(rds.outPrefix + "fileName: [" + fileName + "], fileContent[0-100]: [" + fileContent.substring(0, 100) + "]");

    return fileName + ":" + fileBinary;
}</MCSCRIPTDEFINITION>
