<mx:Code>// Main function: sapc_getReqState

function sapc_getReqState(Par) {
    reqState = sapc_uGetContextVar("SAPC_REQ_STATE");
    if (reqState) {
        uWarning("sapc_getReqState: reqState: [" + reqState + "]");
        return reqState;
    } else {
        var errCount = uGetErrCount();
        uWarning("sapc_getReqState: uGetErrCount() returning: [" + errCount + "]");
        if (errCount &gt; 0) return "ERROR";
        else return "IDLE";
    }
}</mx:Code>
</mx:Function>
<mx:Function Enabled="TRUE" ErrorStatus="0" Script="JScript" name="sapc_getReqStateInfo">
<mx:Code>// Main function: sapc_getReqStateInfo

function sapc_getReqStateInfo(Par) {
    reqStateInfo = sapc_uGetContextVar("SAPC_REQ_STATE_INFO");

    var res = "%$ddm.datetime8601%: Job Finished";

    if (reqStateInfo) {
        uWarning("sapc_getReqState: context SAPC_REQ_STATE_INFO: [" + reqStateInfo + "]");
        res = uReplaceString(reqStateInfo, "!!", "|") + "|" + res;
    }

    var errorCount = uGetErrCount();
    if (errorCount &gt; 0) {
        var errorInfo = uGetErrorInfo();
        uWarning("sapc_getReqState: uGetErrorInfo(): [" + errorInfo + "]");
        res = "%$ddm.datetime8601%: " + errorInfo + "|" + res;
    }

    uWarning("sapc_getReqStateInfo: returning: [" + res + "]");
    return "{A}" + res;
}</mx:Code>
</mx:Function>
