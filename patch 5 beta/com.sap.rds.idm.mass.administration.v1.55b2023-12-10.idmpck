<mx:Function Enabled="TRUE" ErrorStatus="0" Script="JScript" name="sapc_setRunningAndMSKEYVALUEandContext">
<mx:Code>/*
 * Preparing Request and Mass Operation
 *  checking config item SAPC_JOB_SATE for sate (e.g. running)
 *  set running if state is not running
 *  set context (state, job id&amp;guid and notification parameters)
 *
 * Dependencies: sapc_core_scriptHeader, sapc_getRunningRequestMSKEYforJobGuid, sapc_setConfigItem
 * Attention: using "call script in case of error, which is why errors should only be printed at the very end of the script"
 *
 * 2020-10-27 SAP NN: Adaptations for SP4 to store all information on Request object and not user constants or config items anymore
 * 2022-05-11 SAP NN: Store MSKEY for Request in Config item (SAPC_REQUEST_ACTIVE&lt;jobId&gt;, otherwise its hard to determine MSKEY of current running process later
 */

function sapc_setRunningAndMSKEYVALUEandContext(Par) {
    var rds = sapc_core_scriptHeader("sapc_setRunningAndMSKEYVALUEandContext", Par);

    var mskey = Par.get("MSKEY");
    var DATETIME = Par.get("DATETIME");
    var requestorMskey = Par.get("REQUESTOR_MSKEY");

    var activity = Par.get("ACTIVITY");
    var activityQuery = "SELECT ValText FROM mxi_AttrValueHelp WHERE ValId = 'SAPC_REQ_TRIGGER_MASS_CHANGE_ACTIVITY' AND ValKey = '" + activity + "'";
    var activityText = uSelect(activityQuery);
    var errMsg = "";

    var requestorMskeyValue = uIS_sGetValue(requestorMskey, rds.masterIDS, "MSKEYVALUE", 0);

    var activeRunningRequests = sapc_getRunningRequestMSKEYforJobGuid(activity);

    if (activeRunningRequests &gt; 0) { // already one running -&gt; get requestor and compose error
        var execBy = uIS_sGetValue(activeRunningRequests, rds.masterIDS, "SAPC_REQ_REQUESTOR", 0);
        execBy = uIS_sGetValue(execBy, rds.masterIDS, "MSKEYVALUE", 0);
        errMsg += "[" + rds.taskName + "/" + DATETIME + "] Job [" + activityText + "] is already running, started by [" + execBy + "] ";

    } else {

        var jobId = uSelect("select jobId from mc_jobs where mcObsoletedTime is null and JobGuid = '" + activity + "'");
        sapc_uWarning(rds.outPrefix + "ID of job for activity [" + activity + "]: [" + jobId + "]");

        if (!jobId) {
            errMsg += "[" + rds.taskName + "/" + DATETIME + "] Unable to get ID for Job GUID [" + activity + "]. ";

        } else {

            uSetContextVar("SAPC_REQ_STATE", "PREPROCESSING");
            uSetContextVar("SAPC_REQ_STATE_INFO", "[" + rds.taskName + "/" + DATETIME + "] Job [" + activity + "] , started by [" + requestorMskeyValue + "] set to state preprocessing.");
            uSetContextVar("SAPC_REQUEST_ACTIVITY", activity);
            uSetContextVar("SAPC_REQUEST_ACTIVITY_TEXT", activityText);
            //uSetContextVar("SAPC_REQ_JOB_GUID", activity); -&gt; not required anymore, already in activy
            uSetContextVar("SAPC_REQ_JOB_ID", jobId);
            //uIS_SetValue(mskey, rds.masterIDS, "SAPC_REQ_STATE", "PREPROCESSING", rds.taskName); -&gt; happening in next process step
            // 2022-05-11 SAP NN: Store MSKEY for Request in Config item, otherwise its hard to determine MSKEY of current running process later
            // sapc_setConfigItem&lt;Config Item Type&gt;!!&lt;Config Item Id&gt;!!&lt;Config Item Value(s)&gt;
            sapc_setConfigItem("SAPC_REQUEST_ACTIVE!!" + jobId + "!!" + mskey);

        }
    }

    //var uniqueValue = activity + ":" + DATETIME + ":" + requestorMskeyValue;
    // gotta user new String() to get .replace running :(
    var uniqueValue = ("" + activityText).replace(/[!@#$%\^&amp;*(),.?\":{}|&lt;&gt;\/\\]/g, "_") + ":" + DATETIME + ":" + requestorMskeyValue;
    var displayValue = activityText + " started " + DATETIME + " by " + requestorMskeyValue;

    uIS_SetValue(mskey, rds.masterIDS, "DISPLAYNAME", displayValue, rds.taskName);
    uIS_SetValue(mskey, rds.masterIDS, "MSKEYVALUE", uniqueValue, rds.taskName);

    // check for errors
    if (errMsg != "") {

        uSetContextVar("SAPC_REQ_STATE", "ERROR");
        uSetContextVar("#MSG_TEMPLATE", "SAPC_MASS_JOB_ERROR");
        uSetContextVar("#MSG_PAR_SAPC_REQ_STATE_INFO", errMsg);
        uSetContextVar("SAPC_REQ_STATE_INFO", errMsg);
        uSetContextVar("#MSG_PAR_SAPC_REQ_TRIGGER_MASS_CHANGE_ACTIVITY", activityText);
        uSetContextVar("#MSG_RECIPIENTS", requestorMskey);
        uErrMsg(2, rds.outPrefix + errMsg);

    }

}</mx:Code>
</mx:Function>
