<mx:Function Enabled="TRUE" ErrorStatus="0" Script="JScript" name="sapc_getAllAssignmentsOfTypeForRole">
<mx:Code>/*
 * 2023-08-11 SAP NN:	V1 job logic will perform assignments one by one (e.g. Role:PRiv), this script helps to assign all roles/privs to a role in one go
 *						This allows the audit information to only be updated once per role, otherwise the audit information would grow endless
 *						Furthermore assigner and link reason are copied to link
 * In:					Type of RoleID!!assignment (ROLE or PRIVILEGE)!!Operation(ADD or DEL)!!Assigner!!Reason
 * Out:					List of Privs/Roles to be attached to Role
 */

function sapc_getAllAssignmentsOfTypeForRole(Par) {

    var parArray = Par.split("!!");
    var roleID = parArray[0];
    var assType = (parArray[1] == "ROLE") ? "MX_ROLE" : "MX_PRIVILEGE";
    var operation = parArray[2];
    var assigner = parArray[3];
    var reason = parArray[4];
    var nolock = sapc_core_getNolock();
    var sql, missingAOs, retVal;

    /* -------------------------------- INPUT VALIDATION -------------------------------- */

    if (operation == "ADD") {
        // check existence of privs / roles, print error for missing roles
        sql = "select inIdM.mcMSKEYVALUE from ";
        sql += " (select assignment from SAPC_ROLE_UPLOAD_ASSIGN " + nolock + " where assign_type in ";
        if (assType == "MX_ROLE") sql += "('ROLE')";
        else sql += "('PRIV','PRIVILEGE')";
        sql += " and uniqueid = '" + roleID + "' ) tempTable ";
        sql += " LEFT JOIN ";
        sql += " (select mcMSKEYVALUE from idmv_entry_simple " + nolock + " where mcEntryType = '" + assType + "' ) inIdM ";
        sql += " ON UPPER(tempTable.assignment) = inIdM.mcMSKEYVALUE WHERE inIdM.mcMSKEYVALUE IS NULL";

        uInfo("sapc_getAllAssignmentsOfTypeForRole: Query to get missing [" + assType + "] for [" + roleID + "]: [" + sql + "]");
        missingAOs = uSelect(sql);

        if (missingAOs &amp;&amp; missingAOs != "null") {
            uError("sapc_getAllAssignmentsOfTypeForRole: The following Objects do not exist in IDM DB: [" + missingAOs + "]");
        }
    } else {
        // check existence of privs / roles, assignment to role; warning for missing links
        sql = "select inIdM.mcThisMSKEYVALUE from ";
        sql += " (select DELETE_ASSIGNMENT from SAPC_ROLE_UPLOAD_ASSIGN " + nolock + " ";
        sql += " WHERE ((DELETE_ASSIGNMENT IS NOT NULL) AND (DELETE_TYPE IN ";
        if (assType == "MX_ROLE") sql += "('ROLE')";
        else sql += "('PRIV','PRIVILEGE')";
        sql += " ) AND (" + sapc_core_getDBcommands("STRINGLENGTH") + "(DELETE_ASSIGNMENT) &gt; 0)) "; // gotta do not null and length &gt; 0 for different DB types
        sql += " and uniqueid = '" + roleID + "' ) tempTable ";
        sql += " LEFT JOIN ";
        sql += " (select mcThisMSKEYVALUE from idmv_link_ext " + nolock + " where mcOtherMSKEYVALUE = UPPER('" + roleID + "') and mcAttrName = 'MXREF_MX_ROLE' and mcThisOcName = '" + assType + "') inIdM ";
        sql += " ON UPPER(tempTable.DELETE_ASSIGNMENT) = inIdM.mcThisMSKEYVALUE WHERE inIdM.mcThisMSKEYVALUE IS NULL";

        uInfo("sapc_getAllAssignmentsOfTypeForRole: Query to get missing [" + assType + "] for [" + roleID + "]: [" + sql + "]");
        missingAOs = uSelect(sql);

        if (missingAOs &amp;&amp; missingAOs != "null") {
            uWarning("sapc_getAllAssignmentsOfTypeForRole: The following Objects are not linked to role [" + roleID + "] in IdM DB: [" + missingAOs + "]");
        }
    }

    /* -------------------------------- DOING: GET ROLES TO ADD/DEL -------------------------------- */

    if (operation == "ADD") {
        sql = "select mcMSKEY from idmv_entry_simple " + nolock + ", SAPC_ROLE_UPLOAD_ASSIGN " + nolock + " ";
        sql += " where mcEntryType = '" + assType + "' and mcMSKEYVALUE = UPPER (ASSIGNMENT) and assign_type in ";
        if (assType == "MX_ROLE") sql += "('ROLE')";
        else sql += "('PRIV','PRIVILEGE')";
        sql += " and uniqueid = '" + roleID + "'";
    } else {
        sql = "select inIdM.mcThisMSKEY from ";
        sql += " (select DELETE_ASSIGNMENT from SAPC_ROLE_UPLOAD_ASSIGN " + nolock + " ";
        sql += " WHERE ((DELETE_ASSIGNMENT IS NOT NULL) AND (DELETE_TYPE IN ";
        if (assType == "MX_ROLE") sql += "('ROLE')";
        else sql += "('PRIV','PRIVILEGE')";
        sql += " ) AND (" + sapc_core_getDBcommands("STRINGLENGTH") + "(DELETE_ASSIGNMENT) &gt; 0)) "; // gotta do not null and length &gt; 0 for different DB types
        sql += " and uniqueid = '" + roleID + "' ) tempTable ";
        sql += " LEFT JOIN ";
        sql += " (select mcThisMSKEY, mcThisMSKEYVALUE from idmv_link_ext " + nolock + " where mcOtherMSKEYVALUE = UPPER('" + roleID + "') and mcAttrName = 'MXREF_MX_ROLE' and mcThisOcName = '" + assType + "') inIdM ";
        sql += " ON UPPER(tempTable.DELETE_ASSIGNMENT) = inIdM.mcThisMSKEYVALUE WHERE inIdM.mcThisMSKEYVALUE IS NOT NULL";
    }

    uInfo("sapc_getAllAssignmentsOfTypeForRole: Query to get mskeys of for operation [" + operation + "] for [" + assType + "] for [" + roleID + "]: [" + sql + "]");
    var mskeyListAO = uSelect(sql);

    /* -------------------------------- DOING: COMPOSE RETURN VALUE -------------------------------- */

    if (!mskeyListAO || mskeyListAO == "null") {
        uWarning("sapc_getAllAssignmentsOfTypeForRole: No objects for operation [" + operation + "] of type [" + assType + "] for [" + roleID + "]");
        return "{A}";
    } else if (mskeyListAO.indexOf("!ERROR") != -1) {
        uError("sapc_getAllAssignmentsOfTypeForRole: Error when trying to get objects for operation [" + operation + "] of type [" + assType + "] for [" + roleID + "]: [" + mskeyListAO + "]");
        return "{A}";
    } else {

        //TODO: missing del differentiatino
        if (operation == "ADD") {
            var assignPrefix = "{LinkAssigner=" + assigner + "!!LinkReason=" + reason + "}";
            retVal = "{A}" + assignPrefix + uReplaceString(mskeyListAO, "!!", "|" + assignPrefix); // 123!!456 -&gt; {A}{Reason...}123|{Reason...}456
        } else {
            retVal = "{D}" + uReplaceString(mskeyListAO, "!!", "|"); // 123!!!456 -&gt; {D}123|456
        }

        uInfo("sapc_getAllAssignmentsOfTypeForRole: Return value for [" + assType + "] for [" + roleID + "]: [" + retVal + "]");
        return retVal;
    }
}</mx:Code>
</mx:Function>
