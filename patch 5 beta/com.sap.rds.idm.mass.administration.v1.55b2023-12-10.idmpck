<mx:Function Enabled="TRUE" ErrorStatus="0" Script="JScript" name="sapc_checkPackageConstants">
<mx:Code>// Main function: sapc_checkPackageConstants
// 2023-12-10 SAP NN: #81: errorInTemp is not defined

function sapc_checkPackageConstants(Par) {

    importClass(java.io.File);


    var pathUpload = uGetConstant("pck.SAPC_PATH_UPLOAD");
    var pathDownload = uGetConstant("pck.SAPC_PATH_DOWNLOAD");
    var pathTemp = uGetConstant("pck.SAPC_PATH_TEMP");

    var errorMaintenance = "Maintain package constants SAPC_PATH_UPLOAD and SAPC_PATH_DOWNLOAD before running the job." +
        " Also make sure that IdM Dispatcher has appropriate read/write access to these folders";


    if (pathUpload == null || pathUpload.length &lt; 2 ||
        pathDownload == null || pathDownload.length &lt; 2 ||
        pathTemp == null || pathTemp.length &lt; 2) {
        uError(errorMaintenance);
        uStop(errorMaintenance);
    }

    // following is the result from both Windows &amp; Unix servers
    // when the requested folde does not exist
    var shellResultNoFileExist = "No such file or directory";


    var errorInUpload = false;
    var errorInDownload = false;
    var errorInTemp = false;
    var f;

    try {
        f = new File(pathUpload);
        if (f.exists() &amp;&amp; f.isDirectory()) {
            uWarning("Directory [" + pathUpload + "] is valid");
        } else {
            errorInUpload = true;
        }
    } catch (e1) {
        errorInUpload = true;
    }


    try {
        f = new File(pathDownload);
        if (f.exists() &amp;&amp; f.isDirectory()) {
            uWarning("Directory [" + pathDownload + "] is valid");
        } else {
            errorInDownload = true;
        }

    } catch (e2) {
        errorInDownload = true;
    }
    try {
        f = new File(pathTemp);
        if (f.exists() &amp;&amp; f.isDirectory()) {
            uWarning("Directory [" + pathTemp + "] is valid");
        } else {
            errorInDownload = true;
        }

    } catch (e3) {
        errorInTemp = true;
    }


    if (errorInUpload || errorInDownload) {
        var errorMessage = "";

        // both folders inacesseible
        if (errorInUpload &amp;&amp; errorInDownload) {
            errorMessage = "Both Upload Folder: [" + pathUpload + "] and Download Folder: [" + pathDownload +
                "] doesnt exist. Maintain SAPC_PATH_UPLOAD and SAPC_PATH_DOWNLOAD respectively and make sure IdM Dispatcher has appropriate read/write access to these folders.";

            // only upload folder inaccessible
        } else if (errorInUpload) {
            errorMessage = "Upload Folder: [" + pathUpload + "] doesnt exist. Maintain SAPC_PATH_UPLOAD accordingly.";

            // only download folder inaccessible
        } else if (errorInTemp) {
            errorMessage = "Download Folder: [" + pathDownload + "] doesnt exist. Maintain SAPC_PATH_DOWNLOAD accordingly.";
        } else {
            errorMessage = "Templates Folder: [" + pathTemp + "] doesnt exist. Maintain SAPC_PATH_TEMP accordingly.";
        }

        uError(errorMessage);
        uStop(errorMessage);
    }

    // Good to go
    uInfo("Package constants maintained... ");

}</mx:Code>
</mx:Function>
<mx:Function Enabled="TRUE" ErrorStatus="0" Script="JScript" name="sapc_cleanGlobalVariables">
<mx:Code>// Main function: sapc_cleanGlobalVariables

function sapc_cleanGlobalVariables(Par) {
    // The global var is the request type e.g. SAPC_JOB_IDENTITY_UPLOAD
    var globalVar = Par.get("GLOBAL_VAR");

    // Job specific global variables are in the following format
    // SAPC_JOB_STATE_ + &lt;requestType&gt;

    // Clean SAPC_REQUEST job state
    uSetGlobalVar("SAPC_JOB_STATE_" + globalVar, "");

    // Clean SAPC_REQUEST filename
    uSetGlobalVar("SAPC_FILENAME_" + globalVar, "");

    // Clean SAPC_REQUEST mskey
    uSetGlobalVar("SAPC_JOB_REQUEST_" + globalVar, "");

    // Clean Requestor of SAPC_REQUEST
    uSetGlobalVar("SAPC_JOB_REQUESTOR_" + globalVar, "");
}</mx:Code>
