<MCSCRIPTDEFINITION>/*
 * Setting context variables for notification in case of error
 *
 * 2020-10-19 SAP NN: remal of config item logic for SP4
 */

function sapc_writeErrorToContext(Par) {

    mskey = uGetEntryID();
    activity = uIS_nGetValues(mskey, "SAPC_REQ_TRIGGER_MASS_CHANGE_ACTIVITY");

    var currInfo = uGetContextVar("#MSG_PAR_SAPC_REQ_STATE_INFO", "");

    uSetContextVar("SAPC_REQ_STATE", "ERROR");
    uSetContextVar("#MSG_TEMPLATE", "SAPC_MASS_JOB_ERROR");
    if (currInfo == "")
        uSetContextVar("#MSG_PAR_SAPC_REQ_STATE_INFO", "[Job=%$ddm.mcjob%/%$ddm.datetime8601%] Execution Error [" + uGetErrorInfo() + "]");
    uSetContextVar("SAPC_REQ_STATE_INFO", "[Job=%$ddm.mcjob%/%$ddm.datetime8601%] Execution Error [" + uGetErrorInfo() + "]");
    if (uGetContextVar("#MSG_PAR_SAPC_REQ_TRIGGER_MASS_CHANGE_ACTIVITY", "#####") == "#####")
        uSetContextVar("#MSG_PAR_SAPC_REQ_TRIGGER_MASS_CHANGE_ACTIVITY", activity);
    uSetContextVar("#MSG_RECIPIENTS", uGetProvisionUser(uGetAuditID()));

    // added with SP4:
    var isID = uGetIDStore();
    uIS_SetValue(mskey, isID, "SAPC_REQ_STATE", "ERROR", "sapc_writeErrorToContext");
    uIS_SetValue(mskey, isID, "SAPC_REQ_STATE_INFO", "[Job=%$ddm.mcjob%/%$ddm.datetime8601%] Execution Error [" + uGetErrorInfo() + "]", "sapc_writeErrorToContext");

    uSkip(1, 1);
}</MCSCRIPTDEFINITION>
<MCSCRIPTLANGUAGE>JScript</MCSCRIPTLANGUAGE>
<MCSCRIPTLASTCHANGE>1603948276867</MCSCRIPTLASTCHANGE>
<MCSCRIPTNAME>sapc_writeErrorToContext</MCSCRIPTNAME>
<MCSCRIPTSTATUS>0</MCSCRIPTSTATUS>
</SCRIPT>
