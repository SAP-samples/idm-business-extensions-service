<MCSCRIPTDEFINITION>// Checks the state of a job and sets it to error or ok in config item SAPC_JOB_STATE
// Dependencies: sapc_getRequestMSKEY
// 2020-04-06 SAP NN: uGetErrorInfo() can return null, which will cause cannot convert null to an object error when trying thisErr.get(...)
// 2020-10-29 SAP NN: Adaptation for SP4
// 2020-11-16 SAP NN: if (thisErr) -&gt; throwing error if thisErr is DSEEntry | Added try to get job info from syslog

function sapc_setJobState(Par) {

    // wait 1 second in case task chain execution is faster than job execution to avoid overlapping in status update
    uSleep(1000);

    var sql = "";
    var ids = uGetIDStore();
    var script = "sapc_setJobState::";

    var requestMSKEY = sapc_getRequestMSKEY();
    uInfo("sapc_setJobState: Determined MSKEY of request: [" + requestMSKEY + "]");
    //var activity = sapc_getRequestActivity();
    //uInfo("sapc_setJobState: Determined activity of request: [" + activity + "]");

    if (!requestMSKEY) {
        uError(script + "could not find the initiating SAPC_REQUEST mskey");
        return;
    }

    var errCount = uGetErrCount();

    if (errCount &gt; 0) {
        // 2018-05-07 SAP NN: Added as uGetErrorInfo() as text does not always return a text
        // Typical error message: com.sap.idm.ic.ToPassException: Maximum number of profiles for user &lt;user&gt; exceeded
        var thisErr = uGetErrorInfo();
        var errorText = "";
        // if (thisErr) -&gt; if thisErr is DSEEntry -&gt; error
        if (thisErr != null &amp;&amp; thisErr != "") {
            errorText = "" + thisErr.get("err_exception");
            if (!errorText || errorText.toLowerCase() == "null") errorText = "" + thisErr.get("err_ModException");
            if (!errorText || errorText.toLowerCase() == "null") errorText = "" + thisErr.get("err_AddException");
            if (!errorText || errorText.toLowerCase() == "null") errorText = "" + thisErr.get("err_DelException");
            if (!errorText || errorText.toLowerCase() == "null") errorText = "" + thisErr.get("err_SQLWarning");
            if (!errorText || errorText.toLowerCase() == "null") errorText = "" + thisErr.toString();
        } else {
            uWarning("sapc_setJobState:: uGetErrorInfo() returned null");
        }

        if (!errorText || errorText.toLowerCase() == "null") {
            // if no error found, try getting error from job log
            errorText = uSelect("select logtext, ADDINFO from mcv_syslog_ext where logid = (select max (logid) from mcv_syslog_ext where jobid = %$ddm.mcjob% and severity = 2)", " - ");
        }

        if (!errorText || errorText.toLowerCase() == "null") errorText = "N/A";


        uWarning("sapc_setJobState:: errorText: " + errorText);
        retVal = uIS_SetValue(requestMSKEY, ids, "SAPC_REQ_STATE", "ERROR", "Job=%$ddm.mcjob%");
        if (retVal.indexOf("!ERROR") != -1) uErrMsg(2, "sapc_setJobState: Error when updating state of request [" + requestMSKEY + "]: [" + retVal + "]");
        retVal = uIS_SetValue(requestMSKEY, ids, "SAPC_REQ_STATE_INFO", "[Job=%$ddm.mcjob%/%$ddm.datetime8601%] Execution Error [" + errorText + "]", "Job=%$ddm.mcjob%");
        if (retVal.indexOf("!ERROR") != -1) uErrMsg(2, "sapc_setJobState: Error when updating state text of request [" + requestMSKEY + "]: [" + retVal + "]");
        uWarning("sapc_setJobState::[Job=%$ddm.mcjob%/%$ddm.datetime8601%] Execution Error [" + errorText + "]", "Job=%$ddm.mcjob%");
    } else {
        retVal = uIS_SetValue(requestMSKEY, ids, "SAPC_REQ_STATE", "IDLE", "Job=%$ddm.mcjob%");
        if (retVal.indexOf("!ERROR") != -1) uErrMsg(2, "sapc_setJobState: Error when updating state of request [" + requestMSKEY + "]: [" + retVal + "]");
        retVal = uIS_SetValue(requestMSKEY, ids, "SAPC_REQ_STATE_INFO", "[Job=%$ddm.mcjob%/%$ddm.datetime8601%] Job execution complete.", "Job=%$ddm.mcjob%");
        if (retVal.indexOf("!ERROR") != -1) uErrMsg(2, "sapc_setJobState: Error when updating state text of request [" + requestMSKEY + "]: [" + retVal + "]");
    }
}</MCSCRIPTDEFINITION>
