<MCSCRIPTDEFINITION>/*
 * ####-##-## SAP ##:	check input value whether it is empty
 *						if empty and SAPC_JOB_IDENTITY_MODIFY_OVERWRITE is set -&gt; delete value
 *						if empty and SAPC_JOB_IDENTITY_MODIFY_OVERWRITE empty -&gt; return current value
 *						else return new value
 * IN:	&lt;new attr value&gt;!!&lt;user MSKEYVALUE&gt;!!&lt;attr name&gt;
 * 2022-10-24 SAP NN: in case SAPC_JOB_IDENTITY_MODIFY_OVERWRITE is set and attr is sapce ( ) do not return space but nothing
 * 2022-10-24 SAP NN: uIS_getValue wont work for multivalue attributes (e.g. manager), uIS_nGetValue would work unless its no _MEMBER attribute
 */

function sapc_skipAttributeIfEmpty(Par) {

    var script = "sapc_skipAttributeIfEmpty";
    var arrayOfStrings = Par.split("!!");
    var attrValue = arrayOfStrings[0];
    var overwritemode = "%$pck.SAPC_JOB_IDENTITY_MODIFY_OVERWRITE%";
    var result = "";

    if (attrValue.length &lt; 1 || attrValue == " ") {
        if (overwritemode == "X" || overwritemode == "x") {
            // result = attrValue;
            // 2022-10-24 SAP NN: in case SAPC_JOB_IDENTITY_MODIFY_OVERWRITE is set and attr is sapce ( ) do not return space but nothing
            result = "";
        } else {
            var idStore = "%$IDSID%";
            var mskeyvalueCurrentIdentity = arrayOfStrings[1];
            var attrName = arrayOfStrings[2];
            var mskeyCurrentIdentity = uIS_Get(idStore, mskeyvalueCurrentIdentity);
            // attrValue = uIS_GetValue(mskeyCurrentIdentity, idStore, attrName);
            // 2022-10-24 SAP NN: uIS_getValue wont work for multivalue attributes (e.g. manager), uIS_nGetValue would work unless its no _MEMBER attribute
            attrValue = uIS_nGetValues(mskeyCurrentIdentity, attrName, "|");
            var attrValueCheck = attrValue.indexOf("ERROR");
            if (attrValueCheck == -1) {
                result = attrValue;
            } else {
                attrValue = "";
                result = attrValue;
            }
        }
    } else {
        result = attrValue;
    }

    return result;
}</MCSCRIPTDEFINITION>
