<MCSCRIPTDEFINITION>/*
 * 2020-10-27 SAP NN: Get active request for activity (job guid) for jobs to get request MSKEY
 * IN: Request Activity (job guid)
 * Out: MSKEY of active requests
 */

function sapc_getRunningRequestMSKEYforJobGuid(Par) {

    uInfo("sapc_getRunningRequestMSKEYforJobGuid: Started with Par [" + Par + "]");

    var sql = "select ar.mskey from idmv_vna ar %NOLOCK%, idmv_vna rr %NOLOCK% ";
    sql += " where ar.attrName = 'SAPC_REQ_TRIGGER_MASS_CHANGE_ACTIVITY' and ar.sVal like UPPER('" + Par + "') ";
    sql += " and rr.AttrName = 'SAPC_REQ_STATE' and rr.sval in ('PREPROCESSING', 'RUNNING') and ar.mskey = rr.mskey ";

    uInfo("sapc_getRunningRequestMSKEYforJobGuid: SQL to get active MSKEY for [" + Par + "]: [" + sql + "]");

    var mskey = uSelect(sql);

    uInfo("sapc_getRunningRequestMSKEYforJobGuid: result of [" + sql + "]: [" + mskey + "]");

    if (!mskey) return 0;

    if (mskey.indexOf("!ERROR") != -1) {
        uErrMsg(2, "sapc_getRunningRequestMSKEYforJobGuid: Error executing SQL [" + sql + "]: [" + mskey + "]");
        uSkip(1, 2, mskey);
        return -1;
    }

    if (mskey.indexOf("!!") != -1) {
        uErrMsg(2, "sapc_getRunningRequestMSKEYforJobGuid: Retrieved multiple running requests for Guid: [" + Par + "]: [" + mskey + "]");
        uSkip(1, 2, "Retrieved multiple running requests for Guid: [" + Par + "]");
        return -1;
    }

    return mskey;
}</MCSCRIPTDEFINITION>
