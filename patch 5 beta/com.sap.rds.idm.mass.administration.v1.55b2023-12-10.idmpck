<MCSCRIPTDEFINITION>/*---------------------------------------------------------------------------------------------------------------------------
 * Script/Function       : sapc_executeAssignments
 * Category              : local script
 * Short description     : adding / removing assignments from user (mass user to mass priv assignment UI form post processing)
 *----------------------------------------------------------------------------------------------------------------------------
 * Parameter(s) Cat.    : Entry (SAPC_REQUEST)
 * Parameter(s)         :   SAPC_REQ_MASS_USER_ASS_PERS (list of user MSKEYS), SAPC_REQ_MASS_USER_ASS_ADD (list of priv MSKEY to be added)
 *							SAPC_REQ_MASS_USER_ASS_DEL (list of priv MSKEY to be added), SAPC_REQ_MASS_USER_ASS_VALIDFROM &amp; SAPC_REQ_MASS_USER_ASS_VALIDTO (dates)
 *							SAPC_REQ_MASS_USER_ASS_REASON, SAPC_REQ_REQUESTOR (Assigner, mandatory)
 * Return value Cat.    : DSE Entry (Par)
 *
 *----------------------------------------------------------------------------------------------------------------------------
 * Dependencies         : -sapc_core_script_functions
 *----------------------------------------------------------------------------------------------------------------------------
 * Change History       : - 2020-01-31 SAP NN: created
 *						: - 2022-09-01 SAP NN: attrName was not defined globally -&gt; error in checkRetVal
 *--------------------------------------------------------------------------------------------------------------------------*/

var rds;
var errMsg = ""; // !! separated list of error messages, checked in last process step
var assignerMSKEY;
var operationText;
var userMSKEY;
var currAssMSKEY;
var attrName = "";
var addPrefixString = "";
var delPrefixString = "{LINKID=###LINKID###}";

function sapc_executeAssignments(Par) {
    rds = sapc_core_scriptHeader("sapc_executeAssignments", Par);

    var userMSKEYs = Par.get("SAPC_REQ_MASS_USER_ASS_PERS");
    var mskeysAdd = Par.get("SAPC_REQ_MASS_USER_ASS_ADD");
    var mskeysDel = Par.get("SAPC_REQ_MASS_USER_ASS_DEL");
    var validFrom = Par.get("SAPC_REQ_MASS_USER_ASS_VALIDFROM");
    var validTo = Par.get("SAPC_REQ_MASS_USER_ASS_VALIDTO");
    var reason = Par.get("SAPC_REQ_MASS_USER_ASS_REASON");
    assignerMSKEY = Par.get("SAPC_REQ_REQUESTOR");


    // check required parameters (user mskeys and moify user)
    if (!userMSKEYs) {
        if (errMsg) errMsg += "!!";
        errMsg += rds.outPrefix + "Missing mandatory parameter SAPC_REQ_MASS_USER_ASS_PERS";
        uError(rds.outPrefix + "Missing mandatory parameter SAPC_REQ_MASS_USER_ASS_PERS");
    }
    if (!assignerMSKEY) {
        if (errMsg) errMsg += "!!";
        errMsg += rds.outPrefix + "Missing mandatory parameter SAPC_REQ_REQUESTOR";
        uError(rds.outPrefix + "Missing mandatory parameter SAPC_REQ_REQUESTOR");
    }
    // if mandatory parameter is missing, leave script
    if (errMsg != "") {
        sapc_uSetContextVar("SAPC_REQ_STATE", "ERROR");
        sapc_uSetContextVar("SAPC_REQ_STATE_INFO", errMsg);
        return Par;
    }

    if (!mskeysAdd &amp;&amp; !mskeysDel) {
        sapc_uWarning(rds.outPrefix + "Nothing to be added/removed -&gt; Skipping...");
        return Par;
    }


    // template assignment string with placeholders to be replaced later
    // {VALIDFROM=2010-04-01!!VALIDTO=2010-04-30!!LinkAssigner=...!!LinkReason=...}
    if (validFrom) addPrefixString += "VALIDFROM=" + validFrom;
    if (validTo) {
        if (addPrefixString) addPrefixString += "!!";
        addPrefixString += "VALIDTO=" + validTo;
    }
    if (reason) {
        if (addPrefixString) addPrefixString += "!!";
        addPrefixString += "LinkReason=" + reason;
    }
    if (addPrefixString) addPrefixString += "!!";
    addPrefixString = "{" + addPrefixString + "LinkAssigner=" + assignerMSKEY + "}";
    sapc_uInfo(rds.outPrefix + "addPrefixString: [" + addPrefixString + "]");


    // loop through users and add/remove privs
    var userMSKEYsArray = uReplaceString(userMSKEYs, "|", "!!").split("!!");
    for (iuma = 0; iuma &lt; userMSKEYsArray.length; iuma++) {
        userMSKEY = userMSKEYsArray[iuma];
        sapc_uInfo(rds.outPrefix + "Handling user: [" + userMSKEY + "]");

        // loop through privs to be added / removed
        if (mskeysAdd) processAssOperation(mskeysAdd, 0);
        if (mskeysDel) processAssOperation(mskeysDel, 2);

    }

    if (errMsg != "") {
        sapc_uSetContextVar("SAPC_REQ_STATE", "PARTIAL ERROR");
        sapc_uSetContextVar("SAPC_REQ_STATE_INFO", errMsg);
    }

    return Par;
}

function processAssOperation(_assMSKEYs, _operation) {

    if (!_assMSKEYs) {
        sapc_uInfo(rds.outPrefix + "processAssOperation: Parameter assMSKEYs is empty for user [" + userMSKEY + "] on operation [" + _operation + "] -&gt; skipping...");
        return "";
    }

    operationText = "add";
    if (_operation == 2) operationText = "remove";

    var assMSKEYsArray = uReplaceString(_assMSKEYs, "|", "!!").split("!!");
    for (_iuma = 0; _iuma &lt; assMSKEYsArray.length; _iuma++) {
        currAssMSKEY = assMSKEYsArray[_iuma];

        attrName = "MXREF_" + uSelect("select mcEntryType from idmv_entry_simple where mcMSKEY = " + currAssMSKEY);

        sapc_uInfo(rds.outPrefix + "Trying to [" + operationText + "] assignment [" + currAssMSKEY + "] as attribute [" + attrName + "] to user: [" + userMSKEY + "]");

        if (_operation == 0) { // simple add
            //OutString=uIS_SetValue(Int MSKey, Int IStore, String AttName, String AttValue, String userId, Operation (0=add, 2=del));
            checkRetVal(uIS_SetValue(userMSKEY, rds.masterIDS, attrName, addPrefixString + currAssMSKEY, assignerMSKEY, _operation));
        } else {
            // if delete a linkid is required, if assignment is with validity
            var linkIds = uSelect("select mcUniqueID from idmv_link_ext where mcAssignedDirect = 1 and mcThisMSKEY = " + userMSKEY + " and mcOtherMSKEY = " + currAssMSKEY);
            if (!linkIds)
                sapc_uInfo(rds.outPrefix + "Authorization [" + currAssMSKEY + "] not assigned to user: [" + userMSKEY + "] -&gt; skipping...");
            else if (linkIds.indexOf("!ERROR") != -1)
                errMsg += rds.outPrefix + "Error while trying to get linkId of authorization [" + currAssMSKEY + "] on user: [" + userMSKEY + "]: [" + linkIds + "]";
            else {
                var currDelPreStr;
                var linkIdsArray = linkIds.split("!!");
                for (iuii = 0; iuii &lt; linkIdsArray.length; iuii++) {
                    currDelPreStr = uReplaceString(delPrefixString, "###LINKID###", linkIdsArray[iuii]);
                    sapc_uInfo(rds.outPrefix + "Trying removal of [" + currAssMSKEY + "] from user [" + userMSKEY + "] with prefix [" + currDelPreStr + "]");
                    //OutString=uIS_SetValue(Int MSKey, Int IStore, String AttName, String AttValue, String userId, Operation (0=add, 2=del));
                    //var retVal = uIS_SetValue(366, 1, "MXREF_MX_PRIVILEGE", "{LINKID=1018}370", "", 2);
                    checkRetVal(uIS_SetValue(userMSKEY, rds.masterIDS, attrName, currDelPreStr + currAssMSKEY, assignerMSKEY, _operation));
                }
            }
        }
    }
}

function checkRetVal(_retVal) {
    if (_retVal &amp;&amp; _retVal.indexOf("!ERROR") != -1) {
        var tmpErr = rds.outPrefix + "Unable to " + operationText + " Assignment [" + currAssMSKEY + "] as attribute [" + attrName + "] to user: [" + userMSKEY + "]. Error is [" + _retVal + "]";
        if (errMsg) errMsg += "!!";
        errMsg += tmpErr;
        uError(tmpErr);
    } else {
        sapc_uInfo(rds.outPrefix + "Result Audit: [" + _retVal + "]");
    }
}</MCSCRIPTDEFINITION>
