<mx:Function Enabled="TRUE" ErrorStatus="0" Script="JScript" name="sapc_provisionUser">
<mx:Code>/*
 * Dependencies:
 * - sapc_core_getRepositoryPackageID (sap.com.rds.idm.core)
 * - sapc_getRepID (sap.com.rds.idm.core)
 * - sapc_getJobVar
 *
 * 2020-10-29 SAP NN: Adaptations for SP4 (no config items)
 * 2023-06-30 SAP NN: modificaiton of provisionUserToRepository to support AD, where AssignUserMembership wont work | fixed some error output
 */

var rds;

function sapc_provisionUser(Par) {
    rds = sapc_core_scriptHeader("sapc_provisionUser", Par);

    // check actions to perform

    var userid = Par.get("USERID");
    var repository = Par.get("REPOSITORY");
    var create = Par.get("CREATE_"); // CreateUser
    var modify = Par.get("MODIFY_"); // ModifyUser
    var provision = Par.get("PROVISION_"); // AssignUserMembership
    var delete_ = Par.get("DELETE_"); // DeleteUser

    var userMSKEY = uIS_Get(rds.masterIDS, userid);
    if (userMSKEY == null || userMSKEY == "null" || userMSKEY == "" || userMSKEY.indexOf("!ERROR") != -1) {
        uErrMsg(2, rds.outPrefix + "Cannot provision user [" + userid + "]. The user does not exist in IdM database.");
        return "";
    }
    var userAccount = checkUserHasAccount(userid, repository); // check user has account priv on repository (in case of create, mod, prov) | user has no acc priv in case of delete
    var isCreate = false;
    if (create != null &amp;&amp; create != "" &amp;&amp; create.toUpperCase() != "FALSE" &amp;&amp; create != "0") isCreate = true;
    var isModify = false;
    if (modify != null &amp;&amp; modify != "" &amp;&amp; modify.toUpperCase() != "FALSE" &amp;&amp; modify != "0") isModify = true;
    var isDelete = false;
    if (delete_ != null &amp;&amp; delete_ != "" &amp;&amp; delete_.toUpperCase() != "FALSE" &amp;&amp; delete_ != "0") isDelete = true;
    var isProvision = false;
    if (provision != null &amp;&amp; provision != "" &amp;&amp; provision.toUpperCase() != "FALSE" &amp;&amp; provision != "0") isProvision = true;
    if (rds.logLevel &gt;= 2) uErrMsg(1, rds.outPrefix + " isCreate [" + isCreate.toString() + "]; isModify [" + isModify.toString() + "]; isDelete [" + isDelete.toString() + "]; isProvision [" + isProvision.toString() + "]");

    // create OR modify OR delete
    if (isCreate &amp;&amp; isModify) {
        uErrMsg(1, rds.outPrefix + "Modification after creation not necessary. Skipping modify.");
        isModify = false;
    }
    if (isCreate &amp;&amp; isDelete) {
        uErrMsg(2, rds.outPrefix + "Cannot create and delete same time. Skipping entry.");
        return false;
    }
    if (isModify &amp;&amp; isDelete) {
        uErrMsg(1, rds.outPrefix + "Modification before deletion not necessary. Skipping modify.");
        isModify = false;
    }
    if (isProvision &amp;&amp; isDelete) {
        uErrMsg(1, rds.outPrefix + "Provisioning before deletion not necessary. Skipping provisioning.");
        isProvision = false;
    }


    if (isCreate) {

        if (!userAccount) {
            uErrMsg(2, rds.outPrefix + "Cannot create user [" + userid + "] on [" + repository + "]. The user does not have an account there. Assign account privilege [PRIV:" + repository + ":ONLY] in order to start creation process.");
        } else {
            provisionUserToRepository(userMSKEY, repository, "CreateUser");
        }
    } else if (isModify) {

        if (!userAccount) {
            uErrMsg(2, rds.outPrefix + "Cannot modify user [" + userid + "] to [" + repository + "]. The user does not have an account there. Assign account privilege [PRIV:" + repository + ":ONLY] in order to start modify process.");
        } else {
            provisionUserToRepository(userMSKEY, repository, "ModifyUser");
        }
    } else if (isDelete) {
        if (userAccount) {
            uErrMsg(2, rds.outPrefix + "Cannot delete user [" + userid + "] from [" + repository + "]. The user has an account there. Revoke account privilege [PRIV:" + repository + ":ONLY] in order to start deprovisioning process.");
        } else {
            provisionUserToRepository(userMSKEY, repository, "DeleteUser");
        }
    }

    // provision BUT not delete
    if (isProvision) {

        if (!userAccount) {
            uErrMsg(2, rds.outPrefix + "Cannot provision user [" + userid + "] to [" + repository + "]. The user does not have an account there. Assign account privilege [PRIV:" + repository + ":ONLY in order to start provisioning process.");
        } else {
            provisionUserToRepository(userMSKEY, repository, "AssignUserMembership");
        }
    }


}

function checkUserHasAccount(userid_, repository_) {

    var userAccount = "" + uSelect("select * from idmv_link_ext where mcAttrName = 'MXREF_MX_PRIVILEGE' and mcThisMSKEYVALUE = '" + userid_ + "' and mcOtherMSKEYVALUE = 'PRIV:" + repository_ + ":ONLY' and mcExecState in (0,1)");
    if (rds.logLevel &gt;= 2) uErrMsg(1, rds.outPrefix + "checkUserHasAccount: Result of query to find user account: " + userAccount);

    if (userAccount != null &amp;&amp; userAccount != "null" &amp;&amp; userAccount != "" &amp;&amp; userAccount.indexOf("!ERROR") == -1) {
        return true;
    } else {
        return false;
    }
}

function provisionUserToRepository(usermskey_, repository_, action) {

    var curRepPackageID = sapc_core_getRepositoryPackageID(repository_);
    if (rds.logLevel &gt;= 2) uErrMsg(1, rds.outPrefix + "provisionUserToRepository: Package ID of repository [" + repository_ + "]: [" + curRepPackageID + "]");

    if (curRepPackageID == 0) {
        uErrMsg(2, rds.outPrefix + "provisionUserToRepository: Cannot determine package ID for repository: [" + repository_ + "].");
    } else {
        var processToBeCalled;

        // 2023-06-30 SAP NN: modificaiton of provisionUserToRepository to support AD, where AssignUserMembership wont work
        if (uSelect("select mcRepositoryTypeQN from mc_repository %NOLOCK% where rep_name = '" + repository_ + "'").indexOf("reptype.SAPC_ActiveDirectory") != -1) { // Repository is AD -&gt; get Process from corresponding package
            processToBeCalled = uSelect("select TaskGUID from mxp_tasks %NOLOCK% where taskname = 'SAPC_Trigger_Full_Group_Assignment_Providisioning_for_User' and mcPackageID = " + curRepPackageID + " and mcObsoletedBy is null");
        } else {
            processToBeCalled = "" + uGetPackageTaskID(curRepPackageID, action);
        }

        if (rds.logLevel &gt;= 2) uErrMsg(1, rds.outPrefix + "Process for action [" + action + "]: [" + processToBeCalled + "]");

        if (processToBeCalled === null || processToBeCalled == "-1" || processToBeCalled == "0") {
            uErrMsg(2, rds.outPrefix + "provisionUserToRepository: Cannot determine action [" + action + "] for repository: [" + repository_ + "].");

        } else {

            repID = sapc_getRepID(repository_);

            // call reset password task in async mode
            var userInfo = "Initiated via Upload Job by User " + sapc_getJobVar("SAPC_REQ_REQUESTOR");
            var result = uProvision(usermskey_, processToBeCalled, rds.auditID, repID, userInfo, 0);
            if (rds.logLevel &gt;= 2) uErrMsg(1, rds.outPrefix + "Audit ID of action [" + action + "]: [" + result + "]");

            if (result.indexOf("!ERROR:") &gt;= 0) {
                uError(rds.outPrefix + "Calling action [" + action + "] on repository + [" + repository_ + "] returned error: " + result);
            }
        }
    }
}</mx:Code>
</mx:Function>
