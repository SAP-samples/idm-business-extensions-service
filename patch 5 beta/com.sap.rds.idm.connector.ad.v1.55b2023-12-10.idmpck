<mx:attr name="#:">
<mx:value>Filter Users</mx:value>
</mx:attr>
<mx:attr name=":">
<mx:value>delete from sapc_%$rep.$NAME%_user where userid not in
 (select userid from sapc_%$rep.$NAME%_user
  $FUNCTION.sapc_core_getNolock()$$ %$rep.SAPC_LOAD_FILTER_USER%)</mx:value>
</mx:attr>
<mx:attr name="#:">
<mx:value>Filter Groups</mx:value>
</mx:attr>
<mx:attr name=":">
<mx:value>delete from sapc_%$rep.$NAME%_group where cn not in
 (select cn from sapc_%$rep.$NAME%_group
  $FUNCTION.sapc_core_getNolock()$$ %$rep.SAPC_LOAD_FILTER_GROUP%)</mx:value>
</mx:attr>
<mx:attr name="#:">
<mx:value>Filter Assignment Additional Information</mx:value>
</mx:attr>
<mx:attr name=":">
<mx:value>delete from sapc_%$rep.$NAME%_groupAssign &#13;
where (refid not in (select dn from sapc_%$rep.$NAME%_group  $FUNCTION.sapc_core_getNolock()$$))&#13;
or ((groupAssignments not in (select dn from sapc_%$rep.$NAME%_group  $FUNCTION.sapc_core_getNolock()$$)) AND (groupAssignments not in (select dn from sapc_%$rep.$NAME%_user  $FUNCTION.sapc_core_getNolock()$$))) &#13;
</mx:value>
</mx:attr>
</mx:Class>
