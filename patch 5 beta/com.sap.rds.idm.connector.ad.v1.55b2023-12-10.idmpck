<mx:Function Enabled="TRUE" ErrorStatus="0" Script="JScript" name="sapc_triggerFullGroupAssignmentProvisioningForUser">
<mx:Code>/*
 * 2023-06-26 SAP NN:	Trigger SAPC Provisoin Single Group User Membership (F52C7311-44B0-4E3D-85EE-7BA274B9CF20) for every group the user is assigned to on current repository
 *						+ Write user account into context variable DC_ACCOUNTREPNAME
 * Dependencies:		sapc_core_script_functions
 * For Mac Dev:			[]|{}@\
 */

function sapc_triggerFullGroupAssignmentProvisioningForUser(Par) {

    var rds = sapc_core_scriptHeader("sapc_sendNotification", Par);

    var repID = uGetRepositoryID();
    var repositoryName = uGetRepositoryName(repID);
    var userMSKEY = Par.get("MSKEY");
    var taskGuid = Par.get("TASKGUID");
    var auditID = uGetAuditID();
    var accountRepName = Par.get("ACCOUNTREPNAME");

    sapc_uInfo(rds.outPrefix + "Repository Name: [" + repositoryName + "] Repository ID: [" + repositoryName + "]");

    uSetContextVar("DC_ACCOUNTREPNAME", accountRepName);

    var sql = "select mcOtherMSKEY from idmv_link_ext %NOLOCK% where mcOtherMSKEYVALUE like 'PRIV:GROUP:" + repositoryName + ":%' ";
    sql += " and mcThisMSKEY = " + userMSKEY + " and mcAttrName = 'MXREF_MX_PRIVILEGE' and mcExecState = 1 ";
    sapc_uInfo(rds.outPrefix + "SQL to get assigned groups for [" + repositoryName + "] of user : [" + userMSKEY + "]: [" + sql + "]");

    var assignedAdGroups = uSelect(sql);
    sapc_uInfo(rds.outPrefix + "Assigned groups for [" + repositoryName + "] of user : [" + userMSKEY + "]: [" + assignedAdGroups + "]");

    if (!assignedAdGroups || assignedAdGroups.toUpperCase() == "NULL") {
        uWarning(rds.outPrefix + "User [" + userMSKEY + "] has no groups on reposotiry [" + repositoryName + "]. Skipping...");
        return Par;
    }

    if (assignedAdGroups.indexOf("!ERROR") != -1) {
        uError(rds.outPrefix + "Error executin SQL to get user groups: [" + assignedAdGroups + "]");
        uSkip(1, 2, "Error executin SQL to get user groups: [" + assignedAdGroups + "]");
        return -1;
    }

    var assignedAdGroupsArray = assignedAdGroups.split("!!");

    for (aaga = 0; aaga &lt; assignedAdGroupsArray.length; aaga++) {

        //uProvision(&lt;Entry ID&gt;, &lt;Task ID&gt;, &lt;Audit Ref&gt;, &lt;Repository ID&gt;, &lt;Msg&gt;, &lt;Delay&gt;); 
        var retValProv = uProvision(assignedAdGroupsArray[aaga], taskGuid, auditID, repID, "go!", 0);

        if (retValProv.indexOf("!ERROR") != -1) {
            uError(rds.outPrefix + "Error provisioning Task [" + taskGuid + "] for repository [" + repositoryName + "] and group [" + assignedAdGroupsArray[aaga] + "]: [" + retValProv + "]");
        }
        sapc_uWarning(rds.outPrefix + "Provisoining for group [" + assignedAdGroupsArray[aaga] + "] started, child audit: [" + retValProv + "]");
    }

    return Par;
}</mx:Code>
</mx:Function>
