<mx:Code>// Main function: sapc_getTaskGuidByName

function sapc_getTaskGuidByName(Par) {
    var packageID = sapc_core_getRepositoryPackageID("%$rep.$NAME%");
    var taskGuid = uSelect("select TaskGUID from mxp_tasks %NOLOCK% where taskname = '" + Par + "' and mcPackageID = " + packageID + " and mcObsoletedBy is null");
    return taskGuid;
}</mx:Code>
