<mx:attr name="#:">
<mx:value>Filter Users</mx:value>
</mx:attr>
<mx:attr name=":">
<mx:value>delete from sapc_%$rep.$NAME%_user where logonname not in
 (select logonname from sapc_%$rep.$NAME%_user
  $FUNCTION.sapc_core_getNolock()$$ %$rep.SAPC_LOAD_FILTER_USER%)</mx:value>
</mx:attr>
<mx:attr name="#:">
<mx:value>Fliter Roles</mx:value>
</mx:attr>
<mx:attr name=":">
<mx:value>delete from sapc_%$rep.$NAME%_role where uniquename not in
(select uniquename from sapc_%$rep.$NAME%_role $FUNCTION.sapc_core_getNolock()$$ %$rep.SAPC_LOAD_FILTER_ROLE%)</mx:value>
</mx:attr>
<mx:attr name="#:">
<mx:value>Filter Groups</mx:value>
</mx:attr>
<mx:attr name=":">
<mx:value>delete from sapc_%$rep.$NAME%_group where uniquename not in
 (select uniquename from sapc_%$rep.$NAME%_group
  $FUNCTION.sapc_core_getNolock()$$ %$rep.SAPC_LOAD_FILTER_GROUP%)</mx:value>
</mx:attr>
<mx:attr name="#:">
<mx:value>Filter Assignment Additional Information</mx:value>
</mx:attr>
<mx:attr name=":">
<mx:value>delete from sapc_%$rep.$NAME%_groupAssign &#13;
where (refid not in (select SPMLID from sapc_%$rep.$NAME%_group  $FUNCTION.sapc_core_getNolock()$$))&#13;
 or ((groupAssignments not in (select SPMLID from sapc_%$rep.$NAME%_group  $FUNCTION.sapc_core_getNolock()$$)) AND (groupAssignments not in (select SPMLID from sapc_%$rep.$NAME%_user  $FUNCTION.sapc_core_getNolock()$$))) &#13;
</mx:value>
</mx:attr>
<mx:attr name=":">
<mx:value>delete from sapc_%$rep.$NAME%_roleAssign &#13;
where (refid not in (select SPMLID from sapc_%$rep.$NAME%_role  $FUNCTION.sapc_core_getNolock()$$))&#13;
 or ((roleAssignments not in (select SPMLID from sapc_%$rep.$NAME%_group  $FUNCTION.sapc_core_getNolock()$$)) AND (roleAssignments not in (select SPMLID from sapc_%$rep.$NAME%_user  $FUNCTION.sapc_core_getNolock()$$)))</mx:value>
</mx:attr>
</mx:Class>
