                                 <mx:Code>// Main function: sap_getAccountAttribute

function sap_getAccountAttribute(Par){

	repositoryname = uGetRepositoryName();
	CurrentIDStore = uGetIDStore();
	accountAttrName = "ACCOUNT" + repositoryname;
	
	accountAttr = uIS_sGetValue(Par, CurrentIDStore, accountAttrName, 0);
    if (accountAttr != null &amp;&amp; accountAttr.length &gt; 0) {
      uInfo(accountAttrName + " for MSKEY " + Par + ": " + accountAttr);
      return accountAttr;
    } 
	else {
      uError(Par +" privilege has no attribute " + accountAttr);
    }
	
	return "!ERROR: " + accountAttr + " not found.";
}</mx:Code>
                              </mx:Function>
                              <mx:Function Enabled="TRUE" Script="JScript" name="sap_prepareNextEntry">
                                 <mx:Code>// Main function: sap_prepareNextEntry

function sap_prepareNextEntry(Entry){

	var scriptName = "sap_prepareNextEntry: ";
	
	var auditID = uGetAuditID();
	var provUser = uGetProvisionUser(auditID);
	
	// skip the entry if no info for group assignment is contained in the userInfo
	if (provUser==null || provUser == "" | provUser == "0") {
		uSkip(1,1);
	}

	uInfo(scriptName + "auditID=" + auditID + "; provUserInfo=" + provUser);
	var assignInfo = provUser.split("!!");
	
	if (assignInfo.length != 2) {
		uWarning(scriptName + "Group assignment task is called with: " + provUser);
	}
	
	Entry.put("OPERATOR" , assignInfo[0]);
	Entry.put("PRIVMSKEY", assignInfo[1]);
	uInfo(scriptName + Entry);
	
	return Entry;
}</mx:Code>
                              </mx:Function>
