<PASSDOC>MSSQL:
_______

FROM         (SELECT DISTINCT DN, COUNT(DN) AS CDN, MAX(DISTINCT OwnerId) AS OwnerId
                       FROM          (SELECT DISTINCT FingerprINT, COUNT(DN) AS DNCOUNT, DN, MAX(OwnerID) AS OwnerId
                                               FROM          Logentries
                                               WHERE      ((OwnerID =
                                                                          (SELECT     OwnerID
                                                                            FROM          Delta_Defs
                                                                            WHERE      (Name = 'sapc_reco_%$rep.$NAME%_dlt_users'))) OR
                                                                      (OwnerID =
                                                                          (SELECT     OwnerID
                                                                            FROM          Delta_Defs
                                                                            WHERE      (Name = 'sapc_reco_%$rep.$NAME%_idm_users')))) AND Operation &lt; 4
                                               GROUP BY FingerprINT, DN
                                               HAVING      (COUNT(DN) &lt; 2)) AS sometablename1
                       GROUP BY DN
                       HAVING      (COUNT(DN) &lt; 2)) AS sometablename2
WHERE     (OwnerId =
                          (SELECT     OwnerID
                            FROM          Delta_Defs
                            WHERE      (Name = 'sapc_reco_%$rep.$NAME%_dlt_users')))

