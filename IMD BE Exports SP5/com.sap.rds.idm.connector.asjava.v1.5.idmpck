<FROMSQL>select link_ext.mcThisMSKEYVALUE, link_ext.mcOtherMSKEY, link_ext.mcOtherMSKEYVALUE, groupAssign.refid from&#13;
 (select mcThisMSKEYVALUE, mcOtherMSKEY, mcOtherMSKEYVALUE from idmv_link_ext $FUNCTION.sapc_core_getNolock()$$&#13;
   where mcThisOcName = 'MX_PERSON' and mcAttrName = 'MXREF_MX_PRIVILEGE' and mcExecState in (0,1)&#13;
    and mcAssignedDirect = 1 and mcOtherMSKEYVALUE like 'PRIV:GROUP:%$rep.$NAME%:%') link_ext&#13;
 left join&#13;
 (select refid, groupAssignments from sapc_%$rep.$NAME%_groupAssign $FUNCTION.sapc_core_getNolock()$$) groupAssign&#13;
 on UPPER('SPML.SAPUSER.' $FUNCTION.sapc_core_getDBcommands(CONCAT)$$ link_ext.mcThisMSKEYVALUE) = UPPER(groupAssign.groupAssignments)&#13;
  and UPPER(link_ext.mcOtherMSKEYVALUE)&#13;
   = UPPER('PRIV:GROUP:%$rep.$NAME%:' $FUNCTION.sapc_core_getDBcommands(CONCAT)$$ $FUNCTION.sapc_core_getDBcommands(SUBSTRING)$$(groupAssign.refid, 15, 1000))&#13;
 where groupAssignments is null&#13;
 order by link_ext.mcThisMSKEYVALUE&#13;
&#13;
</FROMSQL>
