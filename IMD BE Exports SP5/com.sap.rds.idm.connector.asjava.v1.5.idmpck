                     <mx:DSEJobs>
                        <mx:Sequence>
                           <mx:Step Enabled="TRUE" Key="Create and call single (child-group) assignments"/>
                        </mx:Sequence>
                        <mx:Job name="Create and call single (child-group) assignments">
                           <mx:Description/>
                           <mx:Sequence>
                              <mx:Step Enabled="TRUE" Key="AC177EAF-C23B-475B-9C35-1127B63C0A02"/>
                           </mx:Sequence>
                           <mx:Globals/>
                           <mx:Functions>
                              <mx:Function Enabled="TRUE" ErrorStatus="2" Script="JScript" name="sap_asj_callChildGroupAssignments">
                                 <mx:Code>// Main function: sap_asj_callChildGroupAssignments

/**
 * Assignment of group privileges to a group means the groups will be assigned
 * as parent groups of the MX_GROUP object. However this is not supported by the 
 * SPML schema. Therefore this one assignment of multiple parent groups needs to
 * be converted to multiple single group to child assignments.
 */
function sap_asj_callChildGroupAssignments(Par) {

    var scriptName = "sap_asj_callChildGroupAssignments: ";

    var operation = uGetContextVar("OPERATION");
    uInfo(scriptName + "Operation: " + operation);

    if (operation == "{D}") {
        condition = "assignment.mcLinkState = 0 AND assignment.mcExecState IN (1536, 1537)";
    } else if (operation == "{A}") {
        condition = "assignment.mcLinkState = 1 AND assignment.mcExecState IN (512, 513)";
    } else {
        uError(scriptName + "Invalid or no operation found in context variable: " + operation);
        return;
    }

    /* Get assignments for current entry
     * - in state "pending add" (mcLinkState = 1 &amp; mcExecState = 512 or 513)
     * - in state "pending del" (mcLinkState = 0 &amp; mcExecState = 1536 or 1537)
     * - for specfified repository Y
     * - and privilege type Z
     * - no future assignments (mcChangeNumber != 0)
     * - no duplicated privilege names (-&gt; SELECT DISTINCT) in case of contexts 
     */

    mskey = uGetEntryID();
    repositoryName = uGetRepositoryName();

    var sql = "SELECT DISTINCT assignment.mcOtherMskey, privilegename.mcMSKEYVALUE \
	FROM idmv_value_basic_all repositorynames \
	INNER JOIN idmv_value_basic_all privilegetype ON privilegetype.mskey = repositorynames.mskey \
	INNER JOIN idmv_entry_simple privilegename ON privilegename.mcMSKEY = repositorynames.mskey \
	INNER JOIN mxi_link assignment ON assignment.mcOtherMskey = repositorynames.mskey \
	WHERE assignment.mcThisMskey = " + mskey + " \
		AND (" + condition + " AND assignment.mcChangeNumber != 0) \
		AND repositorynames.attrname = 'MX_REPOSITORYNAME' AND repositorynames.SearchValue = '" + repositoryName + "' \
		AND privilegetype.attrname = 'MX_PRIVILEGE_TYPE'  AND privilegetype.SearchValue = 'GROUP'";

    //result looks like privMskeyValue!!privMskeyValue!!privMskeyValue
    var result = uSelect(sql);

    //uInfo(scriptName + "SQL Query:\n" + sql);
    uInfo(scriptName + "Result: " + result);

    if (result == null || result == "") {
        return;
    }

    var taskIdString = uSelect("SELECT taskid FROM mxp_tasks where taskguid = '103DE627-9DA1-408C-B099-B86961638EC5'");
    if (taskIdString == null || taskIdString == "") {
        uError(scriptName + "Group to group assignment task not found!");
        return;
    }
    var taskId = parseInt(taskIdString);

    var privileges = result.split("!!");
    var rootAuditID = uGetAuditID();
    var repositoryID = uGetRepositoryID();

    for (var i = 0; i &lt; privileges.length; i++) {
        // set the add or delete operator depending on the exec state
        var privAssignment = privileges[i].split("|");
        userInfo = operation + "!!" + privAssignment[0];

        // call the task that does the group to child group assignment in AS Java
        var auditID = UserFunc.uProvision(mskey, taskId, rootAuditID, repositoryID, userInfo, 1);
        uInfo(scriptName + "Called task with " + userInfo + " (" + privAssignment[1] + "); AuditID=" + auditID);
    }

}</mx:Code>
</mx:Function>
