function sap_scim_getMultivalueAttributes(Par) {

    var temp = Par.split("!!");
    var arrayName = temp[0];
    var parameterName = temp[1];
    var separator = temp[2];

    importClass(java.util.Map);

    var entry = getSortedMultiValueAttributesFromDSEEntry();

    var result = "";

    var tempKeyArr = entry.keySet().toArray();

    for (var nextIndex in tempKeyArr) {
        var nextKey = tempKeyArr[nextIndex];
        if (nextKey.indexOf(arrayName) &gt; -1 &amp;&amp; nextKey.indexOf(parameterName) &gt; -1) {
            uInfo("Next entry key: " + nextKey);
            if (result.length &gt; 0) {
                result += separator;
                uInfo("Result was not empty so appending separator. It is now: " + result);
            }
            result += entry.get(nextKey);
            uInfo("After appending the nextKey the result is now: " + result);
        }
    }

    return result;
}</MCSCRIPTDEFINITION>
