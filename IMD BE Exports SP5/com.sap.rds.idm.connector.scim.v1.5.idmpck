SELECT     DN, OwnerId
FROM         (SELECT DISTINCT DN, COUNT(DN) AS CDN, MAX(DISTINCT OwnerId) AS OwnerId
                       FROM          (SELECT DISTINCT FingerprINT, COUNT(DN) AS DNCOUNT, DN, MAX(OwnerID) AS OwnerId
                                               FROM          Logentries
                                               WHERE      ((OwnerID =
                                                                          (SELECT     OwnerID
                                                                            FROM          Delta_Defs
                                                                            WHERE      (Name = 'sapc_reco_%$rep.$NAME%_ra_dlt'))) OR
