<MCSCRIPTDEFINITION>// Main function:sap_scim_buildPhoneNumbers
// Function that build json representation of phoneNumber properties,
//first argument is the mobile 
function sap_scim_buildPhoneNumbers(Par) {

    var phoneNumberValues = Par.split("!!");
    var workPhone = phoneNumberValues[0];
    var mobilePhone = phoneNumberValues[1];

    if ((!phoneNumberValues[0] || phoneNumberValues[0] == "") &amp;&amp; (!phoneNumberValues[1] || phoneNumberValues[1] == "")) {
        return "";
    }

    var phoneNumberTypes = ["work", "mobile"];
    var jsonPhoneArray = [];

    for (var i = 0; i &lt; phoneNumberTypes.length; i++) {
        if (phoneNumberValues[i] &amp;&amp; phoneNumberValues[i] != "") {
            var phoneJsonObject = "{";
            phoneJsonObject += '"type" : "' + phoneNumberTypes[i] + '",';
            phoneJsonObject += '"value" : "' + phoneNumberValues[i] + '"';
            phoneJsonObject += "}";
            jsonPhoneArray.push(phoneJsonObject);
        }
    }

    var result = "[" + jsonPhoneArray.join(",") + "]";
    return result;
}</MCSCRIPTDEFINITION>
<MCSCRIPTLANGUAGE>JScript</MCSCRIPTLANGUAGE>
<MCSCRIPTNAME>sap_scim_buildPhoneNumbers</MCSCRIPTNAME>
<MCSCRIPTSTATUS>0</MCSCRIPTSTATUS>
