<MCSCRIPTDEFINITION>// Main function:sap_scim_getAssignedGroupPrivileges

function sap_scim_getAssignedGroupPrivileges(Par) {
    importClass(java.lang.StringBuffer);
    var temp = Par.split("!!");

    var groupPrivMskey = sap_scim_getSCIMContextVariable("PENDINGPRIVILEGEMSKEY");
    var parentId = sap_scim_getSCIMContextVariable("parentId");

    var accountAttributeName = temp[0];
    var storeId = temp[1];
    var skipId = temp[2];

    var nolock = "";
    if ("%$ddm.databasetype%" == 1) { //MS-SQL
        nolock = "WITH (NOLOCK)";
    }

    /* 
	* - get only assignments (mcLinkType = 2)
	* - get all assignments of current entry X (mcLinkState = 0, mcExecState = 1 &amp; mcDisabled = 0) 
	* - and with assignments in state "pending add" (mcLinkState = 1 &amp; mcExecState = 512 or 513, 
			mcDisabled can be 1 e.g. if the user gets reactivated)
	* - assignments with mcExecState 2 (Rejected) and 4 (Failed) are not included. If a failed
	*	assignment gets retried, the state changes immediately to pending.
	* - for specfified repository Y
	* - and privilege type Z
	* - add member task must have been running for the privilege (mcAddAudit IS NOT NULL)
			-&gt; no future assignments
			-&gt; no assignments for which an approval will be done but approval task is not yet running
	* - no privileges for which an approval is needed/running
	*		mcValidateAddAudit &lt; mcAddAudit &lt;- approval is already done
	*		or mcValidateAddAudit IS NULL &lt;- if no approval is necessary
	* - no duplicate privilege names (-&gt; SELECT DISTINCT) in case of contexts
	*/

    var sql = "SELECT accountAttribute.aValue, entryType.SearchValue FROM idmv_value_basic_all entryType INNER JOIN idmv_value_basic_all accountAttribute ON entryType.MSKEY = accountAttribute.MSKEY AND (entryType.SearchValue = 'MX_GROUP' OR entryType.SearchValue = 'MX_PERSON') AND accountAttribute.AttrName = '" + accountAttributeName + "' AND accountAttribute.IS_ID = " + storeId + " INNER JOIN mxi_link assignment ON assignment.mcThisMSKEY = accountAttribute.MSKEY WHERE assignment.mcOtherMSKEY = " + groupPrivMskey + " AND accountAttribute.aValue &lt;&gt; '" + parentId + "' AND assignment.mcLinkType = 2 AND ((assignment.mcLinkState = 0 AND assignment.mcExecState = 1 AND assignment.mcDisabled = 0) OR (assignment.mcLinkState = 1 AND assignment.mcExecState  IN (512,513)  AND ((assignment.mcAddAudit &gt; assignment.mcValidateAddAudit) OR (assignment.mcAddAudit IS NOT NULL AND assignment.mcValidateAddAudit IS NULL))))";
    var result = uSelect(sql);

    uInfo("sap_scim_getAssignedGroupPrivileges SQL Query:\n" + sql);
    uInfo("sap_scim_getAssignedGroupPrivileges Result: " + result);

    var allPrivsStringBuf = new StringBuffer("[");
    var jsonBody = [];

    if (result != null &amp;&amp; result != "") {
        var resultArray = result.split("!!");
        for (var i = 0; i &lt; resultArray.length; i++) {
            // get a row

            var queryResult = resultArray[i].split("|");
            var memberId = sap_core_escapeSpecialCharacters(queryResult[0]);
            var memberType = queryResult[1];

            if (skipId == memberId) {
                continue;
            }
            if (memberType == "MX_PERSON") {
                memberType = "User";
            } else if (memberType == "MX_GROUP") {
                memberType = "Group";
            } else {
                uError("Unsupported SCIM member type");
                return "";
            }

            var attrBuffer = new StringBuffer();
            attrBuffer.append("{");

            attrBuffer.append('"type": ');
            attrBuffer.append('"' + memberType + '"');
            attrBuffer.append(",");

            attrBuffer.append('"value": ');
            attrBuffer.append('"' + memberId + '"');
            attrBuffer.append("}");

            uInfo("add member with id : " + memberId + " and type: " + memberType);
            jsonBody.push(attrBuffer.toString());
        }
    }
    allPrivsStringBuf.append(jsonBody.join(","));
    allPrivsStringBuf.append("]");
    return allPrivsStringBuf.toString();
}</MCSCRIPTDEFINITION>
<MCSCRIPTLANGUAGE>JScript</MCSCRIPTLANGUAGE>
<MCSCRIPTNAME>sap_scim_getAssignedGroupPrivileges</MCSCRIPTNAME>
<MCSCRIPTSTATUS>0</MCSCRIPTSTATUS>
</SCRIPT>
