/**
 * Parse the "active" status to MX_DISABLED attribute.
 * 
 *
 * @param {Par} Par as String - "active" status(True, False, null)
 * @return {Boolean} Par as Boolean - True if (active == false) or (MX_DISABLED == 1)
 */

function sap_scim_parseActiveStatusToMxDisabled(Par) {
    if (!Par || 0 === Par.length) {
        // null means status "New"
        return "";
    }
    var parValue = Par.toLowerCase();
    if (parValue === "true" || parValue === "1") return "";
    else return "1";
