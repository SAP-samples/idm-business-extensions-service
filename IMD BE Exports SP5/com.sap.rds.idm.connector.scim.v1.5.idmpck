<MCSCRIPTDEFINITION>// Main function: sap_sci_parsePhonesAndAddress

/**
 * Get all addresses and phones and parse the address attributes and phone attributes.
 * 
 *
 * @param {Par} Par as Hash - All attributes
 * @return {Par} Par as Hash - All attributes + mobilePhone, workPhone, homeCountry, homeCity, homePostalCode, homeRegion, homeStreetAddress
 */

function sap_scim_parsePhonesAndAddress(Par) {
    // Parameter Par contains the following :
    // phoneNumbers[0].type=work,  phoneNumbers[0].value=12345
    // phoneNumbers[1].type=mobile,  phoneNumbers[0].value=123567
    // phoneNumbers[i].type == "work") ||  (phoneNumbers[i].type == mobile)

    // Import required Java types.
    importClass(java.util.HashMap);

    if (Par != null &amp;&amp; Par != "") {
        var entryValue;
        var entry = Par;
        if (entry.containsKey("phoneNumbers[0].type") == true) {
            entryValue = entry.get("phoneNumbers[0].type");
            if (entryValue == "work") {
                Par.put("workPhone", entry.get("phoneNumbers[0].value"));
            } else if (entryValue == "mobile") {
                Par.put("mobilePhone", entry.get("phoneNumbers[0].value"));
            }
        }

        if (entry.containsKey("phoneNumbers[1].type") == true) {
            entryValue = entry.get("phoneNumbers[1].type");
            if (entryValue == "work") {
                Par.put("workPhone", entry.get("phoneNumbers[1].value"));
            } else if (entryValue == "mobile") {
                Par.put("mobilePhone", entry.get("phoneNumbers[1].value"));
            }
        }
        //Parse home address
        var address = "";
        if (entry.containsKey("addresses[0].type") == true &amp;&amp; entry.get("addresses[0].type") == "home") {
            address = "addresses[0]";
        } else if (entry.containsKey("addresses[1].type") == true &amp;&amp; entry.get("addresses[1].type") == "home") {
            address = "addresses[1]";
        }
        var addressValue;
        if (address != "") {
            addressValue = entry.get(address + ".country");
            if (addressValue != null) {
                Par.put("homeCountry", addressValue);
            }
            addressValue = entry.get(address + ".locality");
            if (addressValue != null) {
                Par.put("homeCity", addressValue);
            }
            addressValue = entry.get(address + ".postalCode");
            if (addressValue != null) {
                Par.put("homePostalCode", addressValue);
            }
            addressValue = entry.get(address + ".region");
            if (addressValue != null) {
                Par.put("homeRegion", addressValue);
            }
            addressValue = entry.get(address + ".streetAddress");
            if (addressValue != null) {
                Par.put("homeStreetAddress", addressValue);
            }
        }
    }

    return Par;
}</MCSCRIPTDEFINITION>
<MCSCRIPTLANGUAGE>JScript</MCSCRIPTLANGUAGE>
