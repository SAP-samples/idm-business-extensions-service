<MCSCRIPTDEFINITION>// Main function:sap_scim_getEntryMskeyValue

/**
 * Get the MSKEYVALUE of an entry
 * If the entry exists in the IDM, get its mskeyvalue, else get the userName(if not empty), or scim userid if userName is empty
 *
 * @param {Par} Format: %ACC_ATTRIBUTE%!!%SCIM_USERID%!!%USERNAME%!!%IDSTOREID
 * @return {String} MSKEYVALUE of the entry
 */

function sap_scim_getEntryMskeyValue(Par) {
    var temp = Par.split("!!");
    var accountAttribute = temp[0];
    var scimUserId = temp[1];
    var username = temp[2];
    var idStoreId = temp[3];

    var mskey = uIS_sGet(idStoreId, accountAttribute, scimUserId, true);
    if (mskey != null &amp;&amp; mskey.lastIndexOf("!ERROR", 0) !== 0) {
        //The entry with that accountAttribute exists, so get its MSKEYVALUE
        var mskeyValue = uIS_GetValue(mskey, idStoreId, "MSKEYVALUE");
