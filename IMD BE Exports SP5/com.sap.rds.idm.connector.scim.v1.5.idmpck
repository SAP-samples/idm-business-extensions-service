// get account attribute by mskey
function sap_scim_getAccountAttribute(Par) {
	var mskey = Par;
    var repositoryname = uGetRepositoryName();
    var CurrentIDStore = uGetIDStore();
    var accountAttrName = "ACCOUNT" + repositoryname;

    var accountAttr = uIS_sGetValue(mskey, CurrentIDStore, accountAttrName, 0);
    if (accountAttr != null &amp;&amp; accountAttr.length &gt; 0) {
        uInfo(accountAttrName + " for MSKEY " + mskey + ": " + accountAttr);
        return accountAttr;
    } else {
        uError(mskey + " privilege has no attribute " + accountAttr);
    }

    return "!ERROR: " + accountAttr + " not found.";
}</MCSCRIPTDEFINITION>
