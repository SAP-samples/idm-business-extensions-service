<MCSCRIPTDEFINITION>// Main function:sap_scim_getGroupAttributeByGroupId

function sap_scim_getGroupAttributeByGroupId(Par) {
    var temp = Par.split("!!");
    var accountAttributeName = temp[0];
    var groupId = temp[1];
    var idStoreId = temp[2];
    var searchedAttrName = temp[3];

    var sql = "SELECT groupAttribute.aValue FROM idmv_value_basic_all entryType INNER JOIN idmv_value_basic_all accountAttribute ON entryType.MSKEY = accountAttribute.MSKEY AND entryType.SearchValue = 'MX_GROUP' AND accountAttribute.AttrName = '" + accountAttributeName + "' AND accountAttribute.SearchValue = '" + groupId.toUpperCase() + "' AND accountAttribute.IS_ID = " + idStoreId + " INNER JOIN idmv_value_basic_all groupAttribute ON groupAttribute.MSKEY = accountAttribute.MSKEY and groupAttribute.AttrName = '" + searchedAttrName + "'";
    var attrValue = uSelect(sql);
    if (attrValue != null &amp;&amp; attrValue.lastIndexOf("!ERROR", 0) !== 0 &amp;&amp; attrValue !== "") {
        return attrValue;
    } else {
        uError("Attribute with name: " + searchedAttrName + " was not found");
    }
}</MCSCRIPTDEFINITION>
