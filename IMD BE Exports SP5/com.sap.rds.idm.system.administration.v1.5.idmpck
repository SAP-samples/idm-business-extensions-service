function sap_core_getDatabasePrefix(Par){

	var prefix = "";
	
	var dbType = "%$ddm.databasetype%" ;	
	if ( dbType == 1 ) { // MS-SQL
		prefix = "dbo.";
	}
	uInfo("The used prefix for the tables are "+prefix);
	return prefix;

}</mx:Code>
