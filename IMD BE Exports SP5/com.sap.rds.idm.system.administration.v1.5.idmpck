            <mx:Function Enabled="TRUE" Script="JScript" name="sapc_deleteUsersOfSpecificRepository">
               <mx:Code>/*------------------------------------------------------------------------------------------------------------------------*
* Program/script name:  sapc_deleteUsersOfSpecificRepository
*-------------------------------------------------------------------------------------------------------------------------*
* Short description :   Check if user has account a ttribute set for specific repository 
*						and if so delete the account attribute 
*
*-------------------------------------------------------------------------------------------------------------------------*
* Created      Date          By                                                               
* changed      dd-mm-yyyy    Initials    		Description                                                         
* Created      01-03-2012    Kari Redling       initial creation and documentation
* Changed     
*-------------------------------------------------------------------------------------------------------------------------*
* Main Function: sapc_deleteUsersOfSpecificRepository
*
* Argument:  	- mskeyValue
*			- mskey
*			- account
*			- tempAccount
*			- repository
*
* Return value:	
*
* Dependencies:	- global constant: glb.SAP_MASTER_IDS_ID
*
*-------------------------------------------------------------------------------------------------------------------------*/
function sapc_deleteUsersOfSpecificRepository(Par)
{
	var script        = "sapc_deleteUsersOfSpecificRepository::";

	var mskeyValue 	= Par.get("MSKEYVALUE");
	var mskey		= Par.get("MSKEY");
	var account		= Par.get("ACCOUNT");
	var tempAccount	= Par.get("TEMPACCOUNT");
	var repository	= Par.get("REPOSITORY");

	var sql			= "";
	var accountNumber		= "";
	var accountRepository 	= "";

	var currentIdStore	= uGetConstant("glb.SAP_MASTER_IDS_ID");

	// -------------------------------------------------------------
	// 1. get number of account attributes set for specified user
	// -------------------------------------------------------------
	
	sql = "SELECT distinct (mskey) FROM idmv_value_basic_active WHERE AttrName = 'ACCOUNT " + repository + " AND mskey = " + mskey;

	accountNumber = uSelect(sql);

	
	if(accountNumber == 1)
	{
		// ------------------------------------------------------------
		// 2. get name of repository
		// ------------------------------------------------------------

		sql = "SELECT AttrName FROM idmv_value_basic_active WHERE AttrName LIKE 'ACCOUNT%' AND mskey = " + mskey;
		
		// Value "ACCOUNT&lt;REP&gt;" has to be reduced to "&lt;REP&gt;"
		accountRepository 	= uSelect(sql);
		accountRepository 	= uReplaceString(accountRepository, "ACCOUNT", "");

		// if User does only exist in the specified repository
		if(accountRepository == repository)
		{
			var returnValue = uIS_Delete(currentIdStore, mskeyValue);

			if (returnValue == mskey)
			{
				// --&gt; User will be deleted
				uErrMsg(1, "User with MskeyValue '" + mskeyValue + "' [" + mskey + "] has been deleted successfully from IdS!");
			}
			else if (returnValue.indexOf("!ERROR") != -1)
			{
				uErrMsg(2, "User with MskeyValue '" + mskeyValue + "' [" + mskey + "] has not been deleted from IdS!");
			}
			else 			
			{
				uErrMsg(2, "Wrong user with MskeyValue '" + mskeyValue + "' [" + mskey + " / " + returnValue + "] seems to be deleted from IdS!");
			}
		}
	}
}</mx:Code>
            </mx:Function>
            <mx:Function Enabled="TRUE" Script="JScript" name="sapc_checkUsersOfSpecificRepository">
               <mx:Code>/*------------------------------------------------------------------------------------------------------------------------*
* Program/script name:  sapc_checkUsersOfSpecificRepository
*-------------------------------------------------------------------------------------------------------------------------*
* Short description :   ???
*
*-------------------------------------------------------------------------------------------------------------------------*
* Created      Date          By                                                               
* changed      dd-mm-yyyy    Initials    	    Description                                                         
* Created      03-03-2012    D047894          enhanced with documentation
* Changed  
*-------------------------------------------------------------------------------------------------------------------------*
* Main Function: sapc_checkUsersOfSpecificRepository 
*
* Argument:  	      - mskeyValue
*				- mskey
*				- account
*				- tempAccount
*				- repository
*
* Return value:	- 
*
* Dependencies:	- 
*
*-------------------------------------------------------------------------------------------------------------------------*/
function sapc_checkUsersOfSpecificRepository(Par)
{
	var script                    = "sapc_checkUsersOfSpecificRepository::";
	var debugMode                 = 1;

	var mskeyValue                = Par.get("MSKEYVALUE");
	var mskey                     = Par.get("MSKEY");
	var account                   = Par.get("ACCOUNT");
	var repository                = Par.get("REPOSITORY");

	var sql                       = "";
	var accountNumber             = "";

	// ----------------------------------------------------------------
	// 1. get number of account attributes used
	// ----------------------------------------------------------------
	
	sql = "SELECT distinct (mskey) FROM idmv_value_basic_active WHERE AttrName = 'ACCOUNT " + repository + " AND mskey = " + mskey;

	accountNumber = uSelect(sql);
	
	// account attribute does not exist for specified user and repository
	if(accountNumber == 0)
	{
		uErrMsg(2, script + "MskeyValue = [" + mskeyValue
					+ "] ; Account = [" + account
					+ "] ; TempAccount = [" + tempAccount
					+ "] ; Account Number = [" + accountNumber
					+ "] ; Repository = [" + accountRepository + "] - User has no account on specified backend system" );
	}
	// account attribute does exist for specified user and repository
	else if(accountNumber == 1)
	{
		uErrMsg(1, script + "MskeyValue = [" + mskeyValue 
					+ "] ; Account = [" + account
					+ "] ; TempAccount = [" + tempAccount
					+ "] ; Account Number = [" + accountNumber 
					+ "] ; Repository = [" + accountRepository + "] - User has account on specified backend system" );
	}
	// more than one account attribute is not possible!
	else
	{
		uErrMsg(2, script + "MskeyValue = [" + mskeyValue 
					+ "] ; Account = [" + account
					+ "] ; TempAccount = [" + tempAccount
					+ "] ; Account Number = [" + accountNumber 
					+ "] ; Repository = [" + accountRepository + "] - More than one account attributes have been found for user on specified backend system");
	}
}</mx:Code>
