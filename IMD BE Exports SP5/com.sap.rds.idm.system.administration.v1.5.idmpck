<mx:Function Enabled="TRUE" ErrorStatus="0" Script="JScript" name="sapc_checkProvisioningQueueForRepositoryDeletion">
<mx:Code>// Main function: sapc_checkProvisioningQueueForRepositoryDeletion
// The script should check if there remaining items in the provisioning queue for a given repository
// If yes the job has to be stopped and an error message is raised

function sapc_checkProvisioningQueueForRepositoryDeletion(Par) {

    // Input data
    var currentRepository = uGetConstant("pck.SAPC_REMOVE_REPOSITORY_NAME");

    var logLevel = sapc_core_getPackageConstant("SAPC_LOG_LEVEL");
    var SQL = "";
    var result = "";

    // Write current data to the log
    if (logLevel &gt;= 2) {
        UserFunc.uErrMsg(1, "sapc_checkProvisioningQueueForRepositoryDeletion currentRepository: " + currentRepository);
    }

    // Check provisioning queue
    SQL = "select * from MXP_Provision where RepositoryID = (select rep_id from MC_REPOSITORY where rep_name = '" + currentRepository + "')";
    result = uSelect(SQL);
    SQL = "";

    if (result == 0) {
        UserFunc.uErrMsg(1, "sapc_checkProvisioningQueueForRepositoryDeletion: no entries in provisioning queue found. Continue job.");
    } else if (result &gt;= 1) {
        UserFunc.uErrMsg(2, "sapc_checkProvisioningQueueForRepositoryDeletion: still entries in provisioning queue found; stopping job.");
        uStop("Job stopped because there are still some entries related to the current repository in the provisioning queue. Repository cannot be deleted.");
    } else {
        UserFunc.uErrMsg(1, "sapc_checkProvisioningQueueForRepositoryDeletion: unclear status about provisioning queue; stopping job.");
    }

    return "";

}</mx:Code>
</mx:Function>
